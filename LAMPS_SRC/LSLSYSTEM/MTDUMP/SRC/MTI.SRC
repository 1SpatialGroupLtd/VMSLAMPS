	BYTE FUNCTION MTISTR
C
**	MODULE MTISTR
**	IDENT  06JA87
C
C Read single stripe from buffer
C
C Copyright Laser-Scan Laboratories. Ltd., Cambridge, England
C
	IMPLICIT NONE
C
	INCLUDE	'HERE:MTD.CMN'
C
	MTIPTR = MTIPTR+1
	MTISTR = MTIBUF(MTIPTR)
	RETURN
	END
C
	BYTE FUNCTION MTICH
C
**	MODULE MTICH
**	IDENT  23DE86
C
C Read single 6 bit char from buffer (4 packed into 3 stripes)
C
C Copyright Laser-Scan Laboratories. Ltd., Cambridge, England
C
	IMPLICIT NONE
C
	INCLUDE	'HERE:MTD.CMN'
C
C functions called
	INTEGER*4	MTI24B
	BYTE		ICLASC
	BYTE		LIB$EXTZV
C
	IF (PPTR.EQ.0) THEN
	   PBUF = MTI24B()
	   PPTR = 4
	ENDIF
	PPTR = PPTR-1
	MTICH = ICLASC( LIB$EXTZV(PPTR*6,6,PBUF) )
	RETURN
	END
C
	INTEGER*2 FUNCTION MTI16B
C
**	MODULE MTI16B
**	IDENT  23DE86
C
C Read 16 bit word from buffer
C
C Copyright Laser-Scan Laboratories. Ltd., Cambridge, England
C
	IMPLICIT NONE
C
C function called
	BYTE	MTISTR
C
C workspace
	INTEGER*2	W
	BYTE		B(2)
	EQUIVALENCE	(W,B)
C
	B(1) = MTISTR()
	B(2) = MTISTR()
	MTI16B = W
C
	RETURN
	END
C
	INTEGER*4 FUNCTION MTI24B
C
**	MODULE MTI24B
**	IDENT  23DE86
C
C Read 24 bit ICL word from buffer (into longword)
C
C Copyright Laser-Scan Laboratories. Ltd., Cambridge, England
C
	IMPLICIT NONE
C
C function called
	BYTE	MTISTR
C
C workspace
	INTEGER*4	L
	BYTE		B(3)
	EQUIVALENCE	(L,B)
C
	B(3) = MTISTR()
	B(2) = MTISTR()
	B(1) = MTISTR()
	MTI24B = L
C
	RETURN
	END
C
	INTEGER*4 FUNCTION MTI32B
C
**	MODULE MTI32B
**	IDENT  23DE86
C
C Read 32 bit longword from buffer
C
C Copyright Laser-Scan Laboratories. Ltd., Cambridge, England
C
	IMPLICIT NONE
C
C function called
	BYTE	MTISTR
C
C workspace
	INTEGER		I
	INTEGER*4	L
	BYTE		B(4)
	EQUIVALENCE	(L,B)
C
	DO 10 I=1,4
	   B(I) = MTISTR()
10	CONTINUE
	MTI32B = L
C
	RETURN
	END
