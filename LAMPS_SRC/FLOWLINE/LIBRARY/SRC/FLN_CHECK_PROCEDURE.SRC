	INTEGER*4 FUNCTION FLN_CHECK_PROCEDURE(PROCEDURE_ID)
C
***	MODULE	FLN_CHECK_PROCEDURE
***	IDENT	11MY88
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England
C
C Author M.W.S. Reid
C
C 	Created					MWSR    11-May-1988
C
C checks that the specified PROCEDURE exists in the database
C
	IMPLICIT NONE
C
C Parameters
	INCLUDE	'LSL$CMNFLOWLINE:FLOW.PAR'
	INCLUDE	'LSL$CMNFLOWLINE:FLOWLINEMSG.PAR'
C
C Arguments
	CHARACTER*(*)		PROCEDURE_ID
C
C Functions
C
C Local Variables
	INTEGER*4	NUM_ERR
	INTEGER*4	NUM_PROC
C
&RDB&	DATABASE FILENAME 'LSL$FLN_DATABASE:FLOWLINE'
C
	NUM_ERR = 0
C
20	CONTINUE
&RDB&   START_TRANSACTION READ_ONLY WAIT RESERVING PROCEDURE FOR 
&RDB&   SHARED READ
&RDB&	ON ERROR
	   NUM_ERR = NUM_ERR+1
	   IF (NUM_ERR .GT. 5) THEN
	      FLN_CHECK_PROCEDURE = RDB$STATUS
	      GOTO 999
	   ENDIF
C
C try again - probably deadlock
	   CALL LIB$WAIT(FLN_PSE)
	   GOTO 20
&RDB&	END_ERROR
C
	NUM_PROC = 0
&RDB&   GET    	   
&RDB&	   ON ERROR
	      NUM_ERR = NUM_ERR+1
	      IF (NUM_ERR .GT. 5) THEN
	         FLN_CHECK_PROCEDURE= RDB$STATUS
&RDB&            ROLLBACK
	         GOTO 999
	      ENDIF
C
C try again - probably deadlock
&RDB&         ROLLBACK
	      CALL LIB$WAIT(FLN_PSE)
	      GOTO 20
&RDB&	   END_ERROR
&RDB&      NUM_PROC =COUNT OF E IN PROCEDURE 
&RDB&      WITH E.PROCEDURE_ID=PROCEDURE_ID
&RDB&   END_GET
C
	IF (NUM_PROC .EQ. 0) THEN
	   FLN_CHECK_PROCEDURE = FLN__NOSUCHPROC
&RDB&      ROLLBACK
	   GOTO 999
	ENDIF
&RDB&   COMMIT
C
	FLN_CHECK_PROCEDURE = RDB$STATUS
C
999	RETURN
	END
