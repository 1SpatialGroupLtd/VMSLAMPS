	MODULE		MTCOPY_CLD

	DEFINE VERB	MTCOPY

	ROUTINE MTCOPY

	PARAMETER	P1, LABEL = INPUT_DEV_SPEC,
			VALUE(REQUIRED,	TYPE = $FILE),
			PROMPT = "Input device"

	PARAMETER	P1, LABEL = OUTPUT_DEV_SPEC,
			VALUE(REQUIRED,	TYPE = $FILE),
			PROMPT = "Output device"

	QUALIFIER	REWIND_OUTPUT,
			DEFAULT

	QUALIFIER	DENSITY,
			VALUE(REQUIRED, TYPE = $QUOTED_STRING)

	QUALIFIER	APPEND	
