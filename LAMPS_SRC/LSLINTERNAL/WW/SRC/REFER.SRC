C------------------------------------------
C
C	REFER
C
C	List all references
C------------------------------------------
	SUBROUTINE REFER(NMAX)
C
***	MODULE REFER
***	IDENT 22MR84
C
	INCLUDE 'WIREWRAP.HDR'
	NMX=NMAX-1
	DO 20 IPIN=1,255
	DO 10 I=1,NMX
	IF(ISHED(I).EQ.IPIN)CALL LINE_OUT(I,NMX)
10	CONTINUE
20	CONTINUE
	DO 50 ICNO=1,99
		IF (ICPTR(ICNO).EQ.0) GOTO 50
		ICUP = ICNO*256
		DO 40 IPIN=1,ICPINS(ICPTR(ICNO))
			ISH = ICUP+IPIN
			DO 30 I=1,NMX
				IF (ISHED(I).EQ.ISH) CALL
     &					LINE_OUT(I,NMX)
30			CONTINUE
40		CONTINUE
50	CONTINUE
	RETURN
	END
 
