C-------------------------------------------------
C
C	JSTAT
C
C	FIND PIN STATUS OF ITH ENTRY IN SCHEDULE
C-------------------------------------------------
	FUNCTION JSTAT(I)
C
***	MODULE JSTAT
***	IDENT 22MR84
C
	INCLUDE 'WIREWRAP.HDR'
	ICNO=ISHED(I)/256
	IPIN=MOD(ISHED(I),256)
	JSTAT=JCLIST(ICNO,IPIN)
	RETURN
	END
