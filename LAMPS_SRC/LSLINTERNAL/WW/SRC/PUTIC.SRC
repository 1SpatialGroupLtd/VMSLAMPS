C----------------------------------------------------------------
C
C	PUTIC
C
C	Form IC name
C----------------------------------------------------------------
	SUBROUTINE PUTIC (NUMBER,NAME)
C
***	MODULE PUTIC
***	IDENT 22MR84
C
	INTEGER NUMBER
	BYTE NAME(4)
C
C Produce  'IC99' or ' IC9' as the case may be
	ICNO = NUMBER/256
	IF (ICNO.LT.0 .OR. ICNO.GT.99) RETURN
	IF (ICNO.LE.9) ENCODE (4,10,NAME) ICNO
	IF (ICNO.GT.9) ENCODE (4,20,NAME) ICNO
10	FORMAT(' IC',I1)
20	FORMAT('IC',I2)
	RETURN
	END
 
