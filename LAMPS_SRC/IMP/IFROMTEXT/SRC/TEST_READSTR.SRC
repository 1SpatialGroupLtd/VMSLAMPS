	PROGRAM TEST_READSTR
C
***	IDENT	26NO87
***	MODULE	TEST_READSTR
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C

	IMPLICIT NONE
C
C Common blocks
C
***	COMMON/LSL$CMNLSL:READSTR.PAR/
***	COMMON/LSL$CMNLSL:TXTC/
C
C Functions called
C
	LOGICAL		RDCH		! reads a character
	LOGICAL		RDINT		! reads an integer
	INTEGER 	READSTR		! reads a string
	LOGICAL		NODIG
	BYTE 		CH
C
C	
	INTEGER	RET,ACDLEN,N
	CHARACTER*80  ACDSTR
C load up TXTBUF
	CALL LSL_INIT
	TXTBUF(1) = ICHAR('X')
	TXTBUF(2) = ICHAR(' ')
	TXTBUF(3) = ICHAR('3')
	TXTPTR = 3
	CALL BSLN
C
	ACDLEN = READSTR(ACDSTR,,ON_SPACE,.TRUE.,RET)
C
C
C Read the integer
C
	NODIG = RDCH(CH)
	CALL WRITEF('%N',N)
C Return
C
	END
