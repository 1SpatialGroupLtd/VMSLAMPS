	SUBROUTINE WRPCLO(FILE)
C
***	IDENT	18DE87
***	MODULE	WRPCLO
C
C Copyright LaserScan Laboratories Ltd., Cambridge, England
C Author Simon Hancock
C Modified for current IDIFFERENCE by TJ Ibbs, 9 December 1986
C
C create 'frame' overlay, and close the difference plot file
C
	IMPLICIT NONE
C
C argument
C
	INTEGER		FILE		! which one
C
***	COMMON/HERE:FILES/
***	COMMON/HERE:PARAM/
***	COMMON/HERE:CMPIFF/
***	COMMON/HERE:OVRLAY/
***	COMMON/LSL$CMNIFF:IFFHAN/
C
C function called
C
	LOGICAL		IFFMODIFY	! open file for modification
C
C workspace
C
	LOGICAL		OK
	INTEGER		I,J
	REAL		Z(10)
C
C IFF stuff
C
	INTEGER*2	LAY
	INTEGER*2	IOVS
	INTEGER*4	ISEQ
	INTEGER*4	IFSN
	INTEGER*2	ISTAT(4)
	INTEGER*2	NUMPTS
	INTEGER*2	IENDS
C
C find highest layer in input files 1&2
C
	LAY = 1
C
	DO 10 I = 1,2
	   DO 5 J=1,CNTNOS(I)
	      IF (LAYNUM(I,J).GT.LAY) LAY=LAYNUM(I,J)
5	   CONTINUE
10	CONTINUE
C
C open the plot IFF file
C
	OK = IFFMODIFY( PLOT_LUN(FILE),
     &			PLOT_FILE(FILE)(:PLOT_LEN(FILE)),,
     &			PLOT_FID(1,FILE) )
	IF (.NOT.OK) GOTO 9999
C
C create new layer with 1 feature
C
	LAY=LAY+1
	IOVS = 0
	CALL IFFNO(LAY,IOVS)			! output NO
	IFSN=1
	ISEQ=0
	CALL IFFNF(IFSN,ISEQ)
	ISTAT(1)=0
	ISTAT(2)=0
	ISTAT(3)=0
	ISTAT(4)=0
	CALL IFFFS(ISTAT)
	Z(1)=TOTAL_RANGE(1)
	Z(2)=TOTAL_RANGE(3)
	Z(3)=TOTAL_RANGE(1)
	Z(4)=TOTAL_RANGE(4)
	Z(5)=TOTAL_RANGE(2)
	Z(6)=TOTAL_RANGE(4)
	Z(7)=TOTAL_RANGE(2)
	Z(8)=TOTAL_RANGE(3)
	Z(9)=TOTAL_RANGE(1)
	Z(10)=TOTAL_RANGE(3)
	IENDS=0
	NUMPTS=5
	CALL IFFST(Z,NUMPTS,IENDS)
	CALL IFFEF
	CALL IFFEO
	CALL IFFEM
	CALL IFFEJ
C
C and close the file again
C
	CALL IFFCLO( PLOT_LUN(FILE) )
C
9999	RETURN
	END
