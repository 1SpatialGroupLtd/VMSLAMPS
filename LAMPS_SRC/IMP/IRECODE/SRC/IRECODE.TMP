	MODULE 		IRECODE_CLD
	DEFINE VERB 	IRECODE
	ROUTINE		IRECODE
	QUALIFIER	IN_SITU, NEGATABLE , SYNTAX=NO_P1
	PARAMETER	P1, LABEL=IN_FILESPEC, VALUE(REQUIRED, 
				TYPE=$FILE), PROMPT="Input-file"
	PARAMETER	P2, LABEL=OUT_FILESPEC, VALUE(REQUIRED, 
				TYPE=$FILE), PROMPT="Output-file"
	QUALIFIER	LOG, NEGATABLE
	QUALIFIER	FC, VALUE (LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	TCC, VALUE (LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	LAYER, VALUE (LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	AC, VALUE (LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	ALPHABETIC, VALUE (TYPE=$FILE,REQUIRED)
	
	DEFINE		SYNTAX NO_P1
	PARAMETER	P1, LABEL=IN_FILESPEC, VALUE(REQUIRED, 
				TYPE=$FILE), PROMPT="Input File"
	QUALIFIER	LOG, NEGATABLE
	QUALIFIER	FC, VALUE ( LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	LAYER, VALUE ( LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	AC, VALUE ( LIST, TYPE=$QUOTED_STRING)
	
	QUALIFIER	TCC, VALUE ( LIST, TYPE=$QUOTED_STRING)
	QUALIFIER	ALPHABETIC, VALUE (TYPE=$FILE, REQUIRED)
	QUALIFIER	IN_SITU, DEFAULT
