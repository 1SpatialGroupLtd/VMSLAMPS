	PROGRAM RULELIST
C
	IMPLICIT NONE
C
	STRUCTURE /RL/
	   INTEGER*4	KEY
	   INTEGER*2	NEXT
	   INTEGER*2	VALS(25)
	END STRUCTURE
C
	RECORD /RL/ R
C
	INTEGER I,OK
C
C
C
	CALL LSL_INIT()
C
C
	CALL FLWOPN(1,'RULE_LIST.LIS')
C
	CALL FLWSEL(1)
C
C Open rule file for read
C
	OPEN (	UNIT = 2,
     &		FILE = 'VALTAB.RULE',
     &		ORGANIZATION = 'INDEXED',
     &		ACCESS = 'KEYED',
     &		RECORDTYPE = 'FIXED',
     &		FORM = 'UNFORMATTED',
     &		RECL = 14,
     &		KEY = (1:4:INTEGER),
     &		STATUS = 'OLD')
C
C Read first record
C
	READ ( UNIT = 2, KEYGE = 0, KEYID = 0, IOSTAT = OK) R
C
10	IF (OK.NE.0) GOTO 900
C
	CALL EXPAND('%I6',R.KEY)
C
	DO 20 I = 1,25
	   IF (I.LE.5) THEN
	      CALL APPEND('%^W%I5',R.VALS(I))
	   ELSE
	      CALL APPEND('%^W%I3',R.VALS(I))
	   ENDIF
C
	   IF (MOD((I-1),5).EQ.0) THEN
	      CALL APPEND(':')
	   ENDIF
20	CONTINUE
C
	CALL APPEND('     :%^W%N',R.NEXT)
C
	CALL FLWSTR
C
	READ (UNIT = 2, IOSTAT = OK) R
	IF (OK.EQ.0) GOTO 10
C
C Exit
C
900	END
