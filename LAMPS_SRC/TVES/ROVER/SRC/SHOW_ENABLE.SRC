	SUBROUTINE SHOW_ENABLE
C
***	MODULE	SHOW_ENABLE
***	IDENT	13FE89
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England
C
C Author M.W.S. Reid
C
C 	Created					MWSR    22-Jan-1988
C
C deals with SHOW ENABLE command
C
	IMPLICIT NONE
C
C Parameters
C
C Common Blocks
	INCLUDE	'LSL$CMNROVER:ROVCMN.CMN'
	INCLUDE	'LSL$CMNROVER:DRAWIN.CMN'
	INCLUDE	'LSL$CMNROVER:FPPDAT.CMN'
	INCLUDE	'LSL$CMNROVER:RASTED.CMN'
	INCLUDE	'LSL$CMNROVER:SELECT.CMN'
	INCLUDE 'LSL$CMNROVER:RCP.CMN'
C
C Functions
C
C Local Variables
C
520	CALL WRITEF(' ')
	CALL WRITEF('Current status:')
	CALL WRITEF(' ')
	CALL EXPAND('ABSOLUTE       ')
	IF(HAD_ABSOLUTE)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('COLOUR_MAP     ')
	IF(COLOUR_MAP)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('CROSS_CURSOR   ')
	IF(CROSS_CURSOR)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL TTWLIN
	CALL EXPAND('DIGITISE       ')
	IF(DIGITISING)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('DIVIDE         ')
	IF(DIVIDED)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('EDIT           ')
	IF(EDITING)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
C
	CALL TTWLIN
	CALL EXPAND('FLOODING       ')
	IF(FLOODING)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('HATCH          ')
	IF(HADHAT)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('HEIGHT         ')
	IF(TXTOPT)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
C
	CALL TTWLIN
	CALL EXPAND('INTERPOLATION  ')
	IF(INTLIN)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('LEGEND         ')
	IF(PLOT_LEG)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('MASK           ')
	IF(MASK)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
C
	CALL TTWLIN
	CALL EXPAND('OVERRIDE       ')
	IF(HADOVR)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('PATTERN        ')
	IF(PATLIN)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('PICTURE 1      ')
	IF(HADPIC(1))THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
C
	CALL TTWLIN
	CALL EXPAND('PICTURE 2      ')
	IF(HADPIC(2))THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('POSITIONING    ')
	IF(POSOPT)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('REGISTRATION   ')
	IF(REGISTERING)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL TTWLIN
C
	CALL EXPAND('SAMPLE         ')
	IF(HAD_SAMPLE)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('TABLE          ')
	IF(TABLE)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('TEXT           ')
	IF(TEXTON)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL TTWLIN
	CALL WRITEF(' ')
C
C	If registering then also display register specific options
	IF(.NOT.REGISTERING)GOTO 999
C
	CALL EXPAND('DEFAULT RCP_ID ')
	IF(USE_DEFAULT_ID)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('DEFAULT RCP_TYPE')
	IF(USE_DEFAULT_TYPE)THEN
	  CALL APPEND('  On       ')
	ELSE
	  CALL APPEND(' Off       ')
	ENDIF
	CALL APPEND('DEFAULT RCP_Z  ')
	IF(USE_DEFAULT_Z)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL TTWLIN
C
	CALL EXPAND('DEFAULT RCP_WZ ')
	IF(USE_DEFAULT_WZ)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL APPEND('DEFAULT RCP_WXY')
	IF(USE_DEFAULT_WXY)THEN
	  CALL APPEND('   On       ')
	ELSE
	  CALL APPEND('  Off       ')
	ENDIF
	CALL TTWLIN
C
	CALL WRITEF(' ')
C	
999	RETURN
C
	END
