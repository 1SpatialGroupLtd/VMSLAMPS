	SUBROUTINE SHOW_PRESET
C
***	IDENT	23DE88
***	MODULE	SHOW_PRESET
C
C	Copyright : Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Created					    DRC   12-Dec-1988	
C
C	Subroutine to deal with the I2GRID SHOW command.
C
	IMPLICIT NONE
C
C
***	COMMON/HERE:CULTRE/
C
C	Local
	INTEGER*4		I		!counter
	INTEGER*4		LOVAL
	INTEGER*4		HIVAL
	INTEGER*2		LEN
	CHARACTER*10		SPACES
C
	DATA SPACES/'         '/
C
	INTEGER*2		EXPAND
C-------------------------------------------------------------------------
	CALL WRITEF
C	Are there any features with preassigned values?
	IF (NUMBER_CONSTANTS.EQ.1) THEN
	  CALL WRITEF('No features have been pre-assigned a value')
	  CALL WRITEF
	  GOTO 999
	ENDIF
C
C-------------------------------------------------------------------------
	CALL WRITEF
     &	('Features with the following codes have pre-assigned values:')
C
	LOVAL=-1
	HIVAL=-1
	I=-1
100	I=I+1
	IF(I.GT.32767)THEN
	  IF(LOVAL.EQ.-1)GOTO 180
	  IF(HIVAL.EQ.-1)THEN
	    CALL WRITEF('Feature code: %I6          Value: %F',
     &                   LOVAL,SET_VALUES(FCSET_ARRAY(LOVAL)))
	  ELSE
	    LEN = EXPAND('Feature code: %I6 - %N',
     &                  LOVAL,HIVAL)
	    CALL APPEND('%S',SPACES(1:30-LEN))
	    CALL APPEND('Value: %F',
     &                  SET_VALUES(FCSET_ARRAY(LOVAL)))
	    CALL TTWLIN
	  ENDIF	  
	  GOTO 180
	ENDIF
C
	IF(FCSET_ARRAY(I).EQ.0)THEN
150	  IF(LOVAL.EQ.-1)GOTO 100
C
	  IF(HIVAL.EQ.-1)THEN
	    CALL WRITEF('Feature code: %I6          Value: %F',
     &                   LOVAL,SET_VALUES(FCSET_ARRAY(LOVAL)))
	    LOVAL=-1
	    GOTO 100
	  ENDIF
C
	  LEN = EXPAND('Feature code: %I6 - %N',
     &                  LOVAL,HIVAL)
	  CALL APPEND('%S',SPACES(1:30-LEN))
	  CALL APPEND('Value: %F',
     &                  SET_VALUES(FCSET_ARRAY(LOVAL)))
	  CALL TTWLIN
          LOVAL=-1
	  HIVAL=-1
	  GOTO 100
	ENDIF
C
	IF(LOVAL.EQ.-1)THEN
	  LOVAL=I
	  GOTO 100
	ENDIF
C
	IF(FCSET_ARRAY(LOVAL).EQ.FCSET_ARRAY(I))THEN
	  HIVAL=I
	  GOTO 100
	ENDIF
C
	IF(FCSET_ARRAY(LOVAL).NE.FCSET_ARRAY(I))THEN
	  I=I-1
	  GOTO 150
	ENDIF			
C
C------------------------------------------------------------------------
180	CALL WRITEF
	CALL WRITEF 
     &	('Features with the following FSNs  have pre-assigned values:')
	LOVAL=-1
	HIVAL=-1
	I=-1
200	I=I+1
	IF(I.GT.32767)THEN
	  IF(LOVAL.EQ.-1)GOTO 280
	  IF(HIVAL.EQ.-1)THEN
	    CALL WRITEF('FSN         : %I6          Value: %F',
     &                   LOVAL,SET_VALUES(NFSET_ARRAY(LOVAL)))
	  ELSE
	    LEN = EXPAND('FSN         : %I6 - %N',
     &                  LOVAL,HIVAL)
	    CALL APPEND('%S',SPACES(1:30-LEN))
	    CALL APPEND('Value: %F',
     &                  SET_VALUES(NFSET_ARRAY(LOVAL)))
	    CALL TTWLIN
	  ENDIF	  
	  GOTO 280
	ENDIF
C
	IF(NFSET_ARRAY(I).EQ.0)THEN
250	  IF(LOVAL.EQ.-1)GOTO 200
C
	  IF(HIVAL.EQ.-1)THEN
	    CALL WRITEF('FSN         : %I6          Value: %F',
     &                   LOVAL,SET_VALUES(NFSET_ARRAY(LOVAL)))
	    LOVAL=-1
	    GOTO 200
	  ENDIF
C
	  LEN = EXPAND('FSN         : %I6 - %N',
     &                  LOVAL,HIVAL)
	  CALL APPEND('%S',SPACES(1:30-LEN))
	  CALL APPEND('Value: %F',
     &                  SET_VALUES(NFSET_ARRAY(LOVAL)))
	  CALL TTWLIN
          LOVAL=-1
	  HIVAL=-1
	  GOTO 200
	ENDIF
C
	IF(LOVAL.EQ.-1)THEN
	  LOVAL=I
	  GOTO 200
	ENDIF
C
	IF(NFSET_ARRAY(LOVAL).EQ.NFSET_ARRAY(I))THEN
	  HIVAL=I
	  GOTO 200
	ENDIF
C
	IF(NFSET_ARRAY(LOVAL).NE.NFSET_ARRAY(I))THEN
	  I=I-1
	  GOTO 250
	ENDIF			
C
C------------------------------------------------------------------------
C
280	CALL WRITEF
C--------------------------------------------------------------------------
C
999	RETURN
C
	END
