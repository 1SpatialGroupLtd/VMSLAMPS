C arguments:
	INTEGER*2     NUM_ADDR(0:(DTI_DMA_DATA-1)/2)	! DTI array
	INTEGER       OFFSET
C code:
	DTI_GETNUM=NUM_ADDR(OFFSET/2)
	RETURN
C
	END
