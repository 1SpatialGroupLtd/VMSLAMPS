C
C Common holding preset values for IFF Control Bytes
C
	INTEGER*2 NF,FS,TH,ST,EF,NS,CC,CP,TC,CH,
     &		  EO,EJ,VO,SS,SL,TX,RA,NO,MD,EM,
     &		  MH,RO,JB,JP,SH,CS,AC,ZS,HI,CB,
     &		  TS
	INTEGER*2 HASHST,STX,IT,AL,PT
	INTEGER*2 NENTRY,ENTNAM(31)
C
	COMMON/CBC/ NF,FS,TH,ST,EF,NS,CC,CP,TC,CH,
     &		    EO,EJ,VO,SS,SL,TX,RA,NO,MD,EM,
     &     	    MH,RO,JB,JP,SH,CS,AC,ZS,HI,CB,
     &		    TS,
     &		    HASHST,STX,IT,AL,PT,NENTRY
C
	EQUIVALENCE (NF,ENTNAM)
