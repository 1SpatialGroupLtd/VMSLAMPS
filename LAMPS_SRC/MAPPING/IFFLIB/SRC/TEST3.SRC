	PROGRAM TEST3
C
***	IDENT   01DE87
***	MODULE  TEST3
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
***	COMMON/LSL$CMNIFF:IFFHAN/
***	COMMON/LSL$CMNIFF:IFFJUN/
***	COMMON/LSL$CMNIFF:IFFSTR/
C
	INTEGER		IFFOPN
	INTEGER		BLOCKS1,BLOCKS2
	INTEGER*2	IECODE,IELEN
	INTEGER*4	POSE,FSN,ISN
	INTEGER*4	I,TIMES, PREVTIMES
	INTEGER*2	MHDATA(1000)
	BYTE		TCDATA(100)
	
C
	DO 5 I=33,126
5	   TCDATA(I-32)=I
	DO 6 I=1,1000
6	   MHDATA(I)=I
	PREVTIMES=-1
C
	TYPE *,'Terminating LPOSE ?'
	ACCEPT *,POSE
	BLOCKS2 = IFFOPN(2,'FILE2:',POSE)
C
10	   CONTINUE
	   TIMES=LPOSE/(POSE/100)
	   IF (TIMES.NE.PREVTIMES) THEN
	      TYPE *,'LPOSE=',LPOSE,TIMES,'%'
	      PREVTIMES=TIMES
	      ISN=0
	      FSN=FSN+1
	      CALL IFFNF(FSN,ISN)
	      CALL IFFEF
	   ENDIF
	   IF (LPOSE.GT.POSE) GOTO 90
	   CALL IFFMH(MHDATA,1000)
	   CALL IFFTC(TCDATA,94)
	   GOTO 10
C
90	TYPE *,'LPOSE=',LPOSE
	ISN=0
	FSN=FSN+1
	CALL IFFNF(FSN,ISN)
	CALL IFFEF
	CALL IFFEJ
	TYPE *,'LPOSE=',LPOSE
	CALL IFFCLO(2)
	CALL EXIT
	END
