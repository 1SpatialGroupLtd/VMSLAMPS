	$CMD	SOLID			; solid lookup
	$CMD	TEXT			; text lookup
	$CMD	INSERT			; BLOCK insert lookup
	$CMD	EXPAND			; BLOCK expand lookup
	$CMD	POINT,		FII	; point lookup
;
	$CMEND	ENTITY
