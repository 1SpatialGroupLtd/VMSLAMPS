	LOGICAL FUNCTION PROCESS_NTF()

***	IDENT	03AU93
***	MODULE PROCESS_NTF
C
C Copyright Laser-Scan Ltd, Cambridge, England
C
C	Author SG 15-July-1991
C
C	Function is the highest in structure when processing NTF file
C
	IMPLICIT NONE
C
C	.---{  PSEUDO CODE  }---.
C	|			|
C	| open ntf file		|
C	| process ntf header	|
C	| process ntf sections	|
C	| close ntf file	|
C	`-----------------------'
C
C common
	INCLUDE 'HERE:RECORD.CMN'
C
C functions called
	LOGICAL OPEN_NTF
	LOGICAL READ_NTF_HEADER
	LOGICAL READ_NTF_SECTIONS
	LOGICAL	READ_VOLTERM
	LOGICAL	CLOSE_NTF
C
C Workspace
	LOGICAL OK
C
C..............................................................................
C
	OK = .TRUE.
	RECNUM =0					! no records read
	CONT_VOL = .TRUE.
C
	DO WHILE (CONT_VOL.AND.OK)
	   OK = OPEN_NTF()
	   IF (OK) THEN
	      OK = READ_NTF_HEADER()
	   ELSE
	      GOTO 999
	   ENDIF
	   IF (OK) THEN
	      OK = READ_NTF_SECTIONS()
	   ELSE
	      GOTO 999
	   ENDIF
	   IF (OK) THEN
	      OK = READ_VOLTERM()
	   ELSE
	      GOTO 999
	   ENDIF
	   CALL CLOSE_NTF()
	END DO
C
C..............................................................................
C
999	PROCESS_NTF = OK
C
	RETURN
	END
C
