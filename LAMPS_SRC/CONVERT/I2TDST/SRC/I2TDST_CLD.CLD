			VALUE(TYPE=$FILE,REQUIRED)

! file spec

	PARAMETER	P1,VALUE(REQUIRED,TYPE=$FILE),
			PROMPT="Input IFF filename "

	PARAMETER	P2,VALUE(REQUIRED,TYPE=$FILE),
			PROMPT="Output TDST filename "

