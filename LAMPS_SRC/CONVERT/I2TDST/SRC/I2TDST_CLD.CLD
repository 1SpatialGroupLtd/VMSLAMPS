	MODULE		I2TDST_CLD

	DEFINE VERB	I2TDST

        ROUTINE		I2TDST

! qualifiers

	QUALIFIER	FRT,NONNEGATABLE,
			VALUE(TYPE=$FILE,REQUIRED)

