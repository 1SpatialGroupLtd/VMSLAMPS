	MODULE		I2KERNCAM_CLD

	DEFINE VERB	I2KERNCAM

	ROUTINE		I2KERNCAM

	PARAMETER	P1, VALUE(REQUIRED, TYPE=$FILE),
			PROMPT="Input-IFF-file"

	PARAMETER	P2, VALUE(REQUIRED, TYPE=$FILE),
