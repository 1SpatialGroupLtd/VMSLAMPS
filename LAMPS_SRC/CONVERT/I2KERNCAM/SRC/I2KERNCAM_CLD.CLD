	QUALIFIER	FRT, NONNEGATABLE, DEFAULT,
			VALUE(TYPE=$FILE,
				DEFAULT=LSL$FRT:KERN_CAM.FRT;0)

	QUALIFIER	DIAGNOSTICS, NEGATABLE
                                            
