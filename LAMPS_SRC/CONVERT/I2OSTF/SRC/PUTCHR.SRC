	SUBROUTINE PUTCHR(CH)
C
***	IDENT	23JN86
***	MODULE	PUTCHR
C
C	Copyright LaserScan Laboratories Ltd., Cambridge, England
C
C to add another character to the current record
C Author TJ Ibbs, 28 april 1982
C
***	COMMON/HERE:BUF/
C
	BYTE		CH
C
C
	RECPTR = RECPTR+1		! next char
	RECBUF(RECPTR) = CH
C
	IF (RECPTR.EQ.8) CALL PUTREC	! output full record
C
	RETURN
	END
