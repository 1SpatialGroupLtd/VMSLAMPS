
	IDENT	'22AP91'

	MODULE		I2GEN_CLD

	DEFINE VERB	I2GEN

	ROUTINE		I2GEN

	PARAMETER	P1, VALUE(REQUIRED, TYPE=$FILE),
			PROMPT="IFF-file"

	PARAMETER	P2, VALUE(REQUIRED, TYPE=$FILE),
			PROMPT="GENAMAP-file"

	QUALIFIER	PARAMETER, NONNEGATABLE
				VALUE(TYPE=$FILE,
					DEFAULT=GENCODES.DAT;0)

	QUALIFIER	OFFSET,
			NEGATABLE,
			DEFAULT

	DISALLOW	NOT PARAMETER
