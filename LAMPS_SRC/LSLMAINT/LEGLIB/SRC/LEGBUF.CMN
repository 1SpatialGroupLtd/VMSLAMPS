C
C Legenda buffer for reading legendas
C
	PARAMETER	HEAD = 0
	PARAMETER	BODY = 1
C
C these parameters are for use with LEGBLK
C
	INTEGER*2	LEGBLK(0:1)
	INTEGER*2	LEGBUF(0:255,0:1)
C
	COMMON /LEGBUF/ LEGBLK,LEGBUF
C
C LEGBLK holds block nos. of blocks in LEGBUF
C	 (0) - current header block	- HEAD
C	 (1) - current body block	- BODY
C	 both <init -1>
C
C LEGBUF holds data of each block:
C	 2nd index corresponds to index on LEGBLK
