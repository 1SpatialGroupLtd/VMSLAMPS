C
C Minimal LDLIB lookalike library to drive VSLIB
C
	SUBROUTINE LDINIT(I)
C
**	MODULE	LDINIT
**	IDENT	15AU84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C Created					CCB	24-May-1984
C
	IMPLICIT NONE
C
	INTEGER	I
C
	IF (I.EQ.0) THEN
	   CALL VSINIT(0)
	ELSE
	   CALL VSINIT(7)
	ENDIF
	RETURN
	END
C
	SUBROUTINE LDFPPM(I,NULL,DCMAXX,DCMAXY)
C
**	MODULE	LDFPPM
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	INTEGER	I,NULL
	REAL	DCMAXX ,DCMAXY
C
	DCMAXX = 330.0			! mm
	DCMAXY = 270.0
	RETURN
	END
C
	SUBROUTINE UNITS(F)
C
**	MODULE	UNITS
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL	F
C
	CALL VSUNIT(F/10.)	! device units actually cm
	RETURN
	END
