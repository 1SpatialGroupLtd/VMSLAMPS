C
C Minimal LDLIB lookalike library to output coordinates to file
C
	SUBROUTINE LDINIT(I)
C
**	MODULE	LDINIT
**	IDENT	25MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C Created					PGH	24-May-1984
C
	IMPLICIT NONE
C
	INTEGER	I
C
***	COMMON/ASCCMN/
C
	IF (I.EQ.0) THEN
	   CLOSE(UNIT=1)
	ELSE
	   OPEN(UNIT=1,NAME='LSL$ASC:LD0.ASC',TYPE='NEW')
	   ASCSCL = 1.0
	   ASCOFX = 0.0
	   ASCOFY = 0.0
	   ASCTHK = -1.0
	ENDIF
	RETURN
	END
C
	SUBROUTINE LDFPPM(I,NULL,DCMAXX,DCMAXY)
C
**	MODULE	LDFPPM
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	INTEGER	I,NULL
	REAL	DCMAXX ,DCMAXY
C
	DCMAXX = 1000.0			! 1 metre square
	DCMAXY = 1000.0
	RETURN
	END
C
	SUBROUTINE THICK(F)
C
**	MODULE	THICK
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL	F
C
	CHARACTER*60	ASCNAM
	INTEGER		INTTHK
C
***	COMMON/LSL$CMNVIO:EXPC/
***	COMMON/HERE:ASCCMN/
C
	IF (F.EQ.ASCTHK) RETURN		! same as last one
C work out name for new file
	INTTHK=F*ASCSCL*1000.
	CALL EXPAND('%WSLSL$ASC:LD%N.ASC',ASCNAM,INTTHK)
	CLOSE(UNIT=1)
	OPEN(UNIT=1,NAME=ASCNAM,STATUS='UNKNOWN',ACCESS='APPEND')
	ASCTHK = F
	RETURN
	END
C
	SUBROUTINE UNITS(F)
C
**	MODULE	UNITS
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL	F
C
***	COMMON/ASCCMN/
C
	ASCSCL = F
	RETURN
	END
C
	SUBROUTINE ORIGIN(X,Y)
C
**	MODULE	ORIGIN
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL X,Y
C
***	COMMON/ASCCMN/
C
	ASCOFX = X
	ASCOFY = Y
	RETURN
	END
C
C
	SUBROUTINE MOVTO(X,Y)
C
**	MODULE	MOVTO
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL	X,Y
C
***	COMMON/ASCCMN/
C
	WRITE(1,100)X*ASCSCL,Y*ASCSCL
100	FORMAT(' ','0',2F9.3)
	RETURN
	END
C
	SUBROUTINE LINTO(X,Y)
C
**	MODULE	LINTO
**	IDENT	24MY84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
	IMPLICIT NONE
C
	REAL	X,Y
C
***	COMMON/ASCCMN/
C
	WRITE(1,100)X*ASCSCL,Y*ASCSCL
100	FORMAT(' ','1',2F9.3)
	RETURN
	END
