	.TITLE GKS_CALL
	.IDENT 	/24SE96/

;Copyright Laser Scan Laboratories Ltd., Cambridge, England
;Author Clarke Brunt, 31-Jul-1990
;

	.LIBRARY	/LSL$LIBRARY:LSLMACLIB/
	CODE_PSECT	LITES

	$SFDEF				; stack frame definitions
;
; GKS_CALL(routine)
; Calls routine passing the same argument list as our caller had.
;
	.ENTRY	GKS_CALL, ^M<>
	CLRL	R0
	TSTL	4(AP)
	BEQL	10$
	CALLG	@SF$L_SAVE_AP(FP),@4(AP)
10$:	RET

	.END
