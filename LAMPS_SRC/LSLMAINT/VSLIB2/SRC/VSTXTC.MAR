;
; Common block for text input and command decoding in VSLIB
; COMMON /VS_TXTC/ VS_DCPTR,VS_TXTPTR,VS_TXTLIM,VS_TXTBUF(80)
;

	COMMON	VS_TXTC
;
VS_DCPTR:	.WORD	0		; decode pointer
VS_TXTPTR:	.WORD	0		; end of line pointer
VS_TXTLIM:	.WORD	80		; maximum length of TXTBUF
VS_TXTBUF:	.BLKB	80		; buffer
;
	COMMON
