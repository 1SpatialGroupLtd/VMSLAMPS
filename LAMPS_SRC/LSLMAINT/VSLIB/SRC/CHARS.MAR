	.TITLE	CHARS
;
;***************************************************************
;
; CHARS.MAR IS THE SOURCE CODE FOR A VSV11 ASCII CHAR SET.
;
;***************************************************************
	.PSECT	CHARS,PIC,GBL,PAGE
	.ALIGN	PAGE
;
I=	^O40000		;INTENSIFY VECTOR OR POINT.
MSX=	^O20000		;MINUS SIGNS FOR...
MSY=	^O100		;...SHORT FORM VECTOR DATA.

;
	.MACRO	VS_ADDR	ADDR
	.WORD	ADDR - DF_START
	.ENDM
;
; MACRO FOR ASSEMBLING SHORT VECTOR OR REL POINT DATA.
; CALLING ARGUMENTS ARE:
;	A = INTENSIFY BIT, "I" OR "U".
;	B = DELTA X, +/-0 THRU 76.
;	C = DELTA Y,	DITTO
;
	.MACRO	SDAT A,B,C
.IIF	IDN <U> <A> , .I=0	;SET INTENSIFY BIT.
.IIF	IDN <I> <A> , .I=I
.IIF	GE B, .SX=0		;SET X SIGN.
.IIF	LT B, .SX=MSX
.IIF	GE B, .DX=B*^O200	;SCALE X DATA TO <12:7>.
.IIF	LT B, .DX=-<B*^O200>
.IIF	GE C, .SY=0		;SET Y SIGN.
.IIF	LT C, .SY=MSY
.IIF	GE C, .DY=C		;Y DATA STAYS IN <5:0>.
.IIF	LT C, .DY=-<C>
	.WORD	.I+.SX+.DX+.SY+.DY	;; A, B, C
	.ENDM	SDAT
	.MACRO	SDAT4	A,B,C
	SDAT	A,B*2,C*2
	.ENDM	
;
	.MACRO	STOPN
	.WORD	^O173000
	.ENDM
	.MACRO	APNT
	.WORD	^O114000
	.ENDM
