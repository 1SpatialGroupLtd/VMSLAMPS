C
C common block to define the filenames we require
C
C define a maximum file name length
C
	INTEGER				FILE_MAX
	PARAMETER			(FILE_MAX=255)
C
C the filenames themselves
C
	CHARACTER*(FILE_MAX)		OSMH_PARFIL		! parameter file
C
C and their (variable) lengths
C
	INTEGER				OSMH_PARLEN
C
C The common blocks
C
	COMMON /OSMH_NEW_FILES/		OSMH_PARLEN
C
	COMMON /OSMH_NEW_FILES_CH/	OSMH_PARFIL
C
