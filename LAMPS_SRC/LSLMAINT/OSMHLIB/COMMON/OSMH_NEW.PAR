	INTEGER*4	MH_CH_IND_A
	PARAMETER	(MH_CH_IND_A = 10)
C
	INTEGER*4	MH_CH_IND_B
	PARAMETER	(MH_CH_IND_B = 11)
C
	INTEGER*4	MH_CH_IND_C
	PARAMETER	(MH_CH_IND_C = 12)
C
	INTEGER*4	MH_CH_IND_D
	PARAMETER	(MH_CH_IND_D = 13)
C
	INTEGER*4	MH_CH_IND_E
	PARAMETER	(MH_CH_IND_E = 14)
C
	INTEGER*4	MH_CH_IND_F
	PARAMETER	(MH_CH_IND_F = 15)
C
	INTEGER*4	MH_CH_IND_G
	PARAMETER	(MH_CH_IND_G = 16)
C
	INTEGER*4	MH_CH_IND_H
	PARAMETER	(MH_CH_IND_H = 17)
C
	INTEGER*4	MH_CHECK_VALUES
	PARAMETER	(MH_CHECK_VALUES = 18)
C
	INTEGER*4	MH_BYTE_COUNT
	PARAMETER	(MH_BYTE_COUNT = 19)
C
C This remains for compatibility with LITES2
C
	INTEGER*4	MH_CHANGE_IND
	PARAMETER	(MH_CHANGE_IND = 20)
