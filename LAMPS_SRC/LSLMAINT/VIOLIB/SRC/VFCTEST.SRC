	PROGRAM T
	BYTE NULL/0/
	INCLUDE 'CMN:EXPC.CMN'
	BYTE VFCB
	EQUIVALENCE (VFCB,EXPVFC)
C
	TYPE 10
10	FORMAT(' This line has a space at its beginning.')
	TYPE 20
20	FORMAT(
     &    '0And this one has a ''0'' and should be double spaced.')
