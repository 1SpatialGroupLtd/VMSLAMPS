C	CALL EXPAND('A ''1'' should cause a form-feed.')
C	VFCB='1'
C	CALL TTWLIN
C
	CALL WRITEF('This should all')
	CALL EXPAND('		 be on one line.')
	VFCB='+'
	CALL TTWLIN
C
	CALL WRITEF('%$This is prompt format output')
	ACCEPT *,I
C
	CALL WRITEF('AB')
	CALL EXPAND('A')
	VFCB=0
	CALL TTWLIN
	CALL EXPAND('B')
	VFCB=0
	CALL TTWLIN
