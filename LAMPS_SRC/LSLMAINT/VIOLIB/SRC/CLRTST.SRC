	PROGRAM CLRTST
C
	IMPLICIT NONE
C
	INTEGER SIZE1,SIZE2
	PARAMETER (SIZE1=123)
	PARAMETER (SIZE2='12345'X)
C
	BYTE ARRAY1(SIZE1)
	BYTE ARRAY2(SIZE2)
C
	INTEGER VIOCLR
C
	INTEGER IERR
C
	IERR=VIOCLR(ARRAY1,SIZE1)
C
