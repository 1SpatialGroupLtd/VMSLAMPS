	SUBROUTINE LDFPPM(I,NULL,DCMAXX,DCMAXY)
C
***	IDENT	06SE88
***	MODULE	LDFPPM
C
C	Copyright LaserScan Laboratories Ltd., Cambridge, England
C
C LDLIB lookalike for KERN GP1 plotter
C
C to inquire size of plotting area
C
C Author R.W. Russell					 6-Sep-1988
C
	IMPLICIT	NONE
C
	INTEGER		I
	INTEGER		NULL
	REAL		DCMAXX
	REAL		DCMAXY
C
***	COMMON/CMN:GP1CMN/
C
	DCMAXX = (WIN_XMAX - WIN_XMIN) 
	DCMAXY = (WIN_YMAX - WIN_YMIN) 
C
	RETURN
	END

