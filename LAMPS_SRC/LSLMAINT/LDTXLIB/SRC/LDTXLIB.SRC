	SUBROUTINE MOVETO(X,Y)
**	MODULE MOVETO
**	IDENT  07DE78
	ENTRY MOVTO(X,Y)
***	COMMON/LDTXPS/
	PX=X*SCALE+OX
	PY=Y*SCALE+OY
	CALL LDTXTR (IX, IY)
	CALL TXMTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE MOVEBY(X,Y)
**	MODULE MOVEBY
**	IDENT  07DE78
	ENTRY MOVBY(X,Y)
***	COMMON/LDTXPS/
	PX=PX+X*SCALE
	PY=PY+Y*SCALE
	CALL LDTXTR (IX, IY)
	CALL TXMTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE DRAWTO(X,Y)
**	MODULE DRAWTO
**	IDENT  07DE78
	ENTRY LINTO(X,Y)
***	COMMON/LDTXPS/
	PX=X*SCALE+OX
	PY=Y*SCALE+OY
	CALL LDTXTR (IX, IY)
	CALL TXLTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE DRAWBY(X,Y)
**	MODULE DRAWBY
**	IDENT  07DE78
	ENTRY LINBY(X,Y)
***	COMMON/LDTXPS/
	PX=PX+X*SCALE
	PY=PY+Y*SCALE
	CALL LDTXTR (IX, IY)
	CALL TXLTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE LDTXTR (I, J)
**	MODULE LDTXTR
**	IDENT  07DE78
***	COMMON/LDTXPS/
	I = PX*7.0
	J = PY*7.0
	RETURN
	END
C
	SUBROUTINE TKINIT(I,K,J)
**	MODULE TKINIT
**	IDENT  26MY82
	CALL LDINIT(1)
	RETURN
	END
C
	SUBROUTINE LDINIT(N)
**	MODULE LDINIT
**	IDENT  07DE78
***	COMMON/LDTXPS/
	PX=0.0
	PY=0.0
	OX=0.0
	OY=0.0
	SCALE=1.0
	CALL LDTXFF
	CALL TXORGN(501, 370)
	CALL TXSCLE(1.0)
	RETURN
	END
C
	SUBROUTINE ORIGIN(X,Y,N)
**	MODULE ORIGIN
**	IDENT  07DE78
***	COMMON/LDTXPS/
	IF (N .EQ. -1) GOTO 20
	X = X*SCALE
	Y = Y*SCALE
	IF (N .EQ. -2) GOTO 20
	IF (N .EQ.  0) GOTO 15
	X = X+PX
	Y = Y+PY
	GOTO 20
15	X = X+OX
	Y = Y+OY
20	OX=X
	OY=Y
	PX=0.0
	PY=0.0
	RETURN
	END
C
	SUBROUTINE UNITS(S)
**	MODULE UNITS
**	IDENT  07DE78
***	COMMON/LDTXPS/
	SCALE=S
	RETURN
	END
