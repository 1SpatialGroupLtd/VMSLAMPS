	SUBROUTINE MOVETO(X,Y)
**	MODULE MOVETO
**	IDENT  07DE78
	ENTRY MOVTO(X,Y)
***	COMMON/LDTXPS/
	PX=X*SCALE+OX
	PY=Y*SCALE+OY
	CALL LDTXTR (IX, IY)
	CALL TXMTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE MOVEBY(X,Y)
**	MODULE MOVEBY
**	IDENT  07DE78
	ENTRY MOVBY(X,Y)
***	COMMON/LDTXPS/
	PX=PX+X*SCALE
	PY=PY+Y*SCALE
	CALL LDTXTR (IX, IY)
	CALL TXMTO  (IX, IY)
	RETURN
	END
C
	SUBROUTINE DRAWTO(X,Y)
**	MODULE DRAWTO
**	IDENT  07DE78
	ENTRY LINTO(X,Y)
***	COMMON/LDTXPS/
