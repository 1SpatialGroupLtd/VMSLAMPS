C
      DOUBLE PRECISION FUNCTION MLFN (E0,E1,E2,PHI)
C ***	IDENT   25OC95
C ***	MODULE  MLFN
C
C FUNCTION TO COMPUTE CONSTANT (M).
C
      IMPLICIT DOUBLE PRECISION (A-Z)
C
      DATA TWO,FOUR /2.0D0,4.0D0/
C
      MLFN = E0 * PHI - E1 * DSIN (TWO * PHI) + E2 * DSIN (FOUR * PHI)
C
      RETURN
      END

