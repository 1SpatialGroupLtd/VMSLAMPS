      SUBROUTINE PAKD (PAK,DMS)
C ***	IDENT   02DE85
C ***	MODULE  PAKD
C
C SUBROUTINE TO CONVERT PACKED DMS TO UNPACKED DMS ANGLE.
C
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
      REAL REL
      INTEGER DMS(1)
      EQUIVALENCE (REL , INT)
C
      DATA ZERO,CON1,CON2 /0.0D0,1000000.0D0,1000.0D0/
      DATA IBLNK,NEG /' ','-'/
C
      DMS(1) = IBLNK
      IF (PAK .LT. ZERO) DMS(1) = NEG
      CON = DABS (PAK)
      DMS(2) = CON / CON1
      CON = DMOD (CON , CON1)
      DMS(3) = CON / CON2
      REL = DMOD (CON , CON2)
      DMS(4) = INT
      RETURN
C
      END

