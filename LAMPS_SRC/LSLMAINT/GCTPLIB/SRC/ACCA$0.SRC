C *******************************************************************
C * U.S.G.S. GENERAL MAP PROJECTION PACKAGE                         *
C * MODULE II                                                       *
C *******************************************************************
C
CMOD    Converted to Fortran77 (WI/CC 1514)           TJM 25-Oct-1995
C
      SUBROUTINE ACCA (IDP)
C ***	IDENT   25OC95
C ***	MODULE  ACCA 
C
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
C
      COMMON /PRJC/ SOURCE(2,100),TARGET(2,100)
      COMMON /NRMC/ SRCORG(2),TRGORG(2),SCALE
      COMMON /SCRT/ EQN(8,9),AM(2,8),BM(2)
C
      DATA ZERO /0.0D0/
C
      XP = SOURCE(1,IDP) - SRCORG(1)
      YP = SOURCE(2,IDP) - SRCORG(2)
      XG = SCALE * (TARGET(1,IDP)-TRGORG(1))
      YG = SCALE * (TARGET(2,IDP)-TRGORG(2))
      AM(1,1)=XP
      AM(1,2)=YP
      AM(1,4)=-XG*XP
      AM(1,5)=-XG*YP
      AM(2,4)=-XP*YG
      AM(2,5)=-YP*YG
      AM(2,6)=XP
      AM(2,7)=YP
      BM(1)=XG
      BM(2)=YG
      DO 40 I=1,8
      DO 40 J=1,8
      CON=ZERO
      DO 20 K=1,2
      CON=CON+AM(K,I)*AM(K,J)
20    CONTINUE
      EQN(I,J) = EQN(I,J) + CON
40    CONTINUE
      DO 80 I=1,8
      CON=ZERO
      DO 60 J=1,2
      CON=CON+AM(J,I)*BM(J)
60    CONTINUE
      EQN(I,9) = EQN(I,9) + CON
80    CONTINUE
C
      RETURN
      END

