C to edit all the sources).
C
	DOUBLE PRECISION FUNCTION DARCOS(VAL)
C
C ***	IDENT   27OC95
C ***	MODULE  DARCOS
C
	DOUBLE PRECISION VAL,NEWVAL
	NEWVAL=VAL
	IF (DABS(NEWVAL).GT.1.0D0) NEWVAL=DSIGN(1.0D0,NEWVAL)
	DARCOS=DACOS(NEWVAL)
	RETURN
	END
C
	DOUBLE PRECISION FUNCTION DARSIN(VAL)
C
C ***	IDENT   27OC95
C ***	MODULE  DARSIN
C
	DOUBLE PRECISION VAL,NEWVAL
	NEWVAL=VAL
	IF (DABS(NEWVAL).GT.1.0D0) NEWVAL=DSIGN(1.0D0,NEWVAL)
	DARSIN=DASIN(NEWVAL)
	RETURN
	END
