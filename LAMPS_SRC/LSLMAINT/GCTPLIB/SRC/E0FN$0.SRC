C
      DOUBLE PRECISION FUNCTION E0FN (ECCNTS)
C ***	IDENT   25OC95
C ***	MODULE  E0FN
C
C FUNCTION TO COMPUTE CONSTANT (E0).
C
      IMPLICIT DOUBLE PRECISION (A-Z)
C
      DATA QUART,ONE,ONEQ,THREE,SIXT /0.25D0,1.0D0,1.25D0,3.0D0,16.0D0/
C
      E0FN = ONE - QUART * ECCNTS * (ONE + ECCNTS / SIXT *
     .       (THREE + ONEQ * ECCNTS))
C
      RETURN
      END

