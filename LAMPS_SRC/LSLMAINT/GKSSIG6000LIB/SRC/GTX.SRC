	SUBROUTINE	GSCHH(HEIGHT)
C
**	MODULE	GSCHH
**	IDENT	26AP84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C Created				Clarke Brunt	26-April-1984
C
C SET TEXT HEIGHT
C
	IMPLICIT NONE
C
C Arguments:
	REAL	HEIGHT			! text height
C
***	COMMON/GKSESL/
***	COMMON/GKSDAT/
C
C workspace
	INTEGER NCHH
C
	GKSERR = 0
	IF (UPTRN) CALL GUPTRN
	NCHH = HEIGHT*SCALEX
	IF (NCHH.EQ.CCHH) GOTO 999	! already set
	CALL WSCHH(NCHH)
	CCHH = NCHH
999	RETURN
	END
C
C
	SUBROUTINE	GSCHUP(PX,PY)
C
**	MODULE	GSCHUP
**	IDENT	26AP84
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C Created				Clarke Brunt	26-April-1984
C
C SET CHARACTER UP VECTOR
C
	IMPLICIT NONE
C
C Arguments:
	REAL	PX,PY			! up vector
C
***	COMMON/GKSESL/
***	COMMON/GKSDAT/
C
C workspace
	REAL	RX,RY
	INTEGER	IX,IY
	REAL	SUM
C
	GKSERR = 0
	IF (UPTRN) CALL GUPTRN
	RX = PX*SCALEX
	RY = PY*SCALEY
	SUM = ABS(RX)+ABS(RY)
	IX = 32767.0*RX/SUM
	IY = 32767.0*RY/SUM
	IF (IX.EQ.CCHUPX.AND.IY.EQ.CCHUPY) GOTO 999	! already set
	CALL WSCHUP(IX,IY)
	CCHUPX = IX
	CCHUPY = IY
999	RETURN
	END
