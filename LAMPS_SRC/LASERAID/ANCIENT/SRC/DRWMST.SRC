	SUBROUTINE DRWMST
C
C Copyright Laser-Scan Laboratories Ltd,Cambridge,England
C
C Author M.C.Fulford
C
C	Created 16/iii/78
C
C To draw the minimum spanning tree on the TX
C
***	COMMON/PARAMS/
C
***	COMMON/RSE/
***	COMMON/LINKS/
C
	DO 200 I = 1,NESIZE
	  K = NODE(I)
	  IF (K .LE. 0) GOTO 200
	  CALL TXMTO(NEBUF(2,I), NEBUF(3,I))
	  CALL TXLTO(NEBUF(2,K), NEBUF(3,K))
200	 CONTINUE
C
	CALL TXEND
	RETURN
	END
