	SUBROUTINE MA01A(A,B,M,N,M1,IA,IB)
	IMPLICIT DOUBLE PRECISION (A-H),(O-Z)
	DIMENSION A(IA,1),B(IB,1),IND(10)
	MM=M-1
10	AMAX=0.0
	DO 30 I=1,M
	  IND(I)=I
	  IF(DABS(A(I,1))-AMAX)30,30,20
20	  AMAX=DABS(A(I,1))
	  I4=I
30	  CONTINUE
	DO 160 J=1,MM
	  IF(I4-J)80,80,40
40	  ISTO=IND(J)
	  IND(J)=IND(I4)
	  IND(I4)=ISTO
	  DO 50 K=1,M
	    STO=A(I4,K)
	    A(I4,K)=A(J,K)
	    A(J,K)=STO
50	    CONTINUE
	  IF(N)80,80,60
60	  DO 70 K=1,N
	    STO=B(I4,K)
	    B(I4,K)=B(J,K)
	    B(J,K)=STO
70	    CONTINUE
80	  AMAX=0.0
	  J1=J+1
	  DO 150 I=J1,M
	    A(I,J)=A(I,J)/A(J,J)
	    DO 110 K=J1,M
	      A(I,K)=A(I,K)-A(I,J)*A(J,K)
	      IF (K-J1)90,90,110
90	      IF(DABS(A(I,K))-AMAX)110,110,100
100	      AMAX=DABS(A(I,K))
	      I4=I
110	      CONTINUE
120	    IF(N)150,150,130
130	    DO 140 K=1,N
	      B(I,K)=B(I,K)-A(I,J)*B(J,K)
140	      CONTINUE
150	    CONTINUE
160	  CONTINUE
	IF(N)230,230,170
170	DO 220 I1=1,M
	  I=M+1-I1
	  DO 210 J=1,N
	    IF(M-I)200,200,180
180	    I2=I+1
	    DO 190 K=I2,M
	      B(I,J)=B(I,J)-A(I,K)*B(K,J)
190	      CONTINUE
200	    B(I,J)=B(I,J)/A(I,I)
210	    CONTINUE
220	  CONTINUE
230	RETURN
	END
