	PROGRAM GENDAT
	BYTE T(8),D(9)
	OPEN(UNIT=1,NAME='IDENT.SRC',EXTENDSIZE=1,
     *     CARRIAGECONTROL='LIST')
	CALL TIME(T(1))
	CALL DATE(D(1))
	WRITE(1,100)
	WRITE(1,101)
	WRITE(1,102)T,D
	WRITE(1,103)
	CALL EXIT
C
100	FORMAT('	SUBROUTINE IDENT(V)')
101	FORMAT('	BYTE V(1)')
102	FORMAT('	CALL WRITEF(''Laseraid [%S] of ',
     *     8A1,' ',9A1,''',V)')
103	FORMAT('	RETURN',/,'	END')
C
	END
