	SUBROUTINE WINDUP
C
C Copyright Laser-Scan Laboratories Ltd,Cambridge,England
C
C Author M.C.Fulford
C
C	Last edited 15/vi/79
C
C To close down properly
C
***	COMMON/PARAMS/
C
***	COMMON/DIGC/
C
	INTEGER RC
C
C
	CALL PEND(1)
	IF (INOVER) CALL NEWOVR(.TRUE.)  ! to output NF 0 & EO
	CALL IFFEJ
	CALL NEWTIM(TSTOP)
	CALL PRTTIM(0)
	CALL SETGUD(.FALSE.)
	CALL TIDY(IFFLUN)
	CALL LINK(2,RC,.FALSE.)
	CALL LDINIT(0)
	CALL MSGF('Bye!')
	CALL EXIT
C
	END
