	SUBROUTINE GETAB(CP,A,B)
C
C Calculate polynomial from corner points
C
	REAL CP(4,4)	! corner points
	REAL A(0:3),B(0:3)	! polynomials
C
	A(0) = CP(1,2)
	A(1) = CP(1,3)-CP(1,2)
	A(2) = CP(1,1)-CP(1,2)
	A(3) = CP(1,4)-CP(1,1)+CP(1,2)-CP(1,3)
C
	B(0) = CP(2,2)
	B(1) = CP(2,3)-CP(2,2)
	B(2) = CP(2,1)-CP(2,2)
	B(3) = CP(2,4)-CP(2,1)+CP(2,2)-CP(2,3)
C
	RETURN
	END
