	.ASK L LTT
	.ASK S STTOUT
.;
	SET /UIC=[100,5]
	.IFF L .GOTO 200
	ADC LTT = [100,6]LTT
	F4P LTT = LTT
	PIP LTT.FTN;/DE
	TKB @[100,6]LTT.TKB
	.IFINS LTT REM LTT
	.IFNINS LTT INS LTT
.;
.200:	.IFF S .GOTO 300
	ADC STT=[100,6]STT,STTOUT
	F4P STT=STT
	PIP STT.FTN;/DE
	TKB @[100,6]STT.TKB
	.IFINS STT REM STT
	.IFNINS STT INS STT
.;
.300:	PIP LTT.*,STT.*/PU
	TIME
