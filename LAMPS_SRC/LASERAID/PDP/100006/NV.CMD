.ENABLE SUBSTITUTION
.SETS $BLANK ""
.SETN $N 0
.BA:.INC $N
.ASKS FILE FILE '$N'
.SETS FIL'$N' "'FILE'"
.IF FILE = $BLANK .GOTO MA
NED 'FILE'
.ASK T PURGE
.ASK LIST'$N' LIST
.IFT T PIP 'FILE'/PU
.GOTO BA

.MA:.DEC $N
.SETN LIM $N
.SETN $N 0
.MM:.INC $N
.IF $N > LIM .GOTO ZA
.SETS S FIL'$N'
; '$N'.	'S'
.IFT LIST'$N' LST 'S'
.GOTO MM

.ZA:TIM
