	PROGRAM PROT
***	COMMON/TIMING/
	CALL ASNLUN(5,'TI',0)
	TIMGAP = 1	! enable timing
	CALL SUB0
	CALL PROTYP
	CALL EXIT
	END
C
	SUBROUTINE SUB0
	DO 100 I = 1,500
	DO 50 J = 1,50
	CALL SUB1
	CALL SUB2
50	CONTINUE
	CALL SUB3
	CALL SUB4
100	CONTINUE
	RETURN
	END
C
	SUBROUTINE SUB1
	DO 10 I = 1,30
	N = N+1
10	CONTINUE
	RETURN
	END
C
	SUBROUTINE SUB2
	DO 10 I = 1,30
	N = N+1
10	CONTINUE
	RETURN
	END
C
	SUBROUTINE SUB3
	DO 10 I = 1,30
	N = N+1
10	CONTINUE
	RETURN
	END
C
	SUBROUTINE SUB4
	DO 10 I = 1,30
	N = N+1
10	CONTINUE
	RETURN
	END
