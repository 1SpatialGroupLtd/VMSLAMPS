C
C Auxiliary data for the line-fitting version of BUNCH
C
	COMMON /MPAUX/ FITPMP(2),FITLEN,SX,SY,SXX,SXY,SYY,
     &		       SINTH,COSTH,SINSAV,COSSAV,XORIG,YORIG
C
	REAL	  FITPMP	! possible new master point
	REAL	  FITLEN	! dist. of PMP from last master point
	REAL	  SX,SY		! sums for line fitting
	REAL	  SXX,SXY,SYY
	REAL	  SINTH,COSTH	! current best vector
	REAL	  SINSAV,COSSAV	! previous best vector
	REAL	  XORIG,YORIG	! current best origin for vector
C
