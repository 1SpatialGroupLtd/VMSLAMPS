	LOGICAL FUNCTION LABEL_CMD()
C
***	MODULE	LABEL_CMD
***	IDENT	25JA89
C
C Copyright Laser-Scan Laboratories Limited, Cambridge, England.
C
C	Created						TJH 23-Dec-1987
C
C Routine to LABEL up various data
C
	IMPLICIT NONE
C W/s
	INTEGER		CMD     ! decoded command number
C
C Parameters:
	INCLUDE 	'LSL$DTMCREATESYS:MESPAR.PAR'
	INCLUDE 	'LSL$CMNLSL:LSLLIBMSG.PAR'
C
C Common:
	INCLUDE 	'LSL$CMNTRIEDIT:COMMAND.CMN'
	INCLUDE 	'LSL$CMNLSL:EXCEPTION.CMN'
	INCLUDE 	'LSL$CMNTRIEDIT:ANNOT.CMN'
	INCLUDE 	'LSL$CMNTRIEDIT:TTTYPE.CMN'
	INCLUDE 	'LSL$CMNLSL:EXPC.CMN'
	INCLUDE 	'LSL$CMNLSL:CMDCOM.CMN'
C
C Functions:
	LOGICAL  	RDCOMM	! read command from TXTBUF
C
C Command table:
	EXTERNAL	LABEL_CMD_TABLE
	INTEGER		LABEL_CMD_TABLE
C
C ......................................................................
C
	LABEL_CMD=.TRUE.
C
	CMD=RDCOMM(LABEL_CMD_TABLE)	! get command
C
C Error?
	IF (ERRNUM.NE.LSL__NORMAL) THEN
	   LABEL_CMD=.FALSE.
	   IF (ERRNUM.EQ.LSL__UNEXPCMD) THEN
	      CALL EXPAND('%S unexpected %S',WARNING,CMDNST)
	      CALL OUTOPT(EXPDSC)
	   ELSEIF (ERRNUM.EQ.LSL__AMBIG) THEN
	      CALL EXPAND('%S ambiguous command %S',WARNING,CMDNST)
	      CALL OUTOPT(EXPDSC)
	   ELSEIF (ERRNUM.EQ.LSL__UNEXPEOL) THEN
	      GOTO 10
	   ENDIF
	   GOTO 20
	ENDIF
	CMD=CMD+1
	GOTO (
     &		10,	! error/nothing
     &		30,	! big
     &		40,	! float
     &		50,	! hts
     &		60,	! integer
     &		70,	! none
     &		80,	! sequence
     &		90,	! small
     &		100	! strings
     &			), CMD	
C
C Command number out of range
	CALL OUTOPT('Command not yet implemented')
	LABEL_CMD=.FALSE.
	GOTO 120
C
C ......................................................................
C Error
10	CALL OUTOPT(ERROR//'Specifying command LABEL')
20	CALL OUTOPT(' ')
	CALL OUTOPT('Available LABEL command qualifiers are:')
	CALL OUTOPT(' ')
	CALL OUTOPT(
     &	'BIG    FLOAT    HEIGHTS   INTEGER    '//
     &			'NONE    SEQUENCE    SMALL')
	CALL OUTOPT(' ')
	CALL OUTOPT('STRINGS')
	CALL OUTOPT(' ')
	LABEL_CMD=.FALSE.
	GOTO 110
C
C ......................................................................
C LABEL BIG
30	KMULT=2
	GOTO 110
C
C ......................................................................
C LABEL FLOAT
40	NODECP=-1
	GOTO 110
C
C ......................................................................
C LABEL HEIGHT
50	LAB_HT=.TRUE.
	GOTO 110
C
C ......................................................................
C LABEL INTEGER
60	NODECP=1
	GOTO 110
C
C ......................................................................
C LABEL NONE
70	LAB_HT=.FALSE.
	LAB_SEQ=.FALSE.
	LAB_STR=.FALSE.
	GOTO 110
C
C ......................................................................
C LABEL SEQUENCE
80	LAB_SEQ=.TRUE.
	GOTO 110
C
C ......................................................................
C LABEL SMALL
90	KMULT=1
	GOTO 110
C
C ......................................................................
C LABEL STRINGS
100	LAB_STR=.TRUE.
110	IF (VT100) CALL POINTS(2)	! Update the tell-back area
120	RETURN
	END
