C
C LITES2 Common block to hold timing information
C
	PARAMETER TIMREAD=1	! time in read-in and draw
	PARAMETER TIMDRAW=2	! time in draw
	PARAMETER TIMFIND=3	! time in find/search
	PARAMETER MAXTIM=3	! number of timing fields
C
	REAL TIMING(MAXTIM)	! held in seconds
C
	REAL STATIM(MAXSTA)	! Times in each state
C
	COMMON/TIMING/TIMING,STATIM
C
