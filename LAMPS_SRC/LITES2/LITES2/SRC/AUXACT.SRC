	LOGICAL FUNCTION AUXACT
C
***	IDENT	17SE87
***	MODULE	AUXACT
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Created					RWR	17-Sep-1987
C
C Description
C
C To test if there are any auxiliary interactive lines active
C
	IMPLICIT NONE
C
***	COMMON/CMN:WORKS/
C w/s
	INTEGER		I
C
	AUXACT = .FALSE.
C
	DO 10, I=1,AUX_LINES
	   IF (AUX_ACTIVE(I)) THEN
	      AUXACT = .TRUE.
	      GOTO 999
	   ENDIF
   10	CONTINUE
C
  999	RETURN
	END
