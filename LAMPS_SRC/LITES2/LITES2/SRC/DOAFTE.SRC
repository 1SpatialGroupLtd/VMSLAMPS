	SUBROUTINE DOAFTE
C
***	IDENT	06JL90
***	MODULE	DOAFTE
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Created					RWR	 9-Nov-1987
C
C Description
C
C	LITES2 cartographic editor AFTER command.
C
	IMPLICIT NONE
C
***     PARAMETER/CMN:MOANMESS/		! moan constants
C
***	COMMON/CMN:AFTER/		! common block
					! to hold command lines
***	COMMON/CMN:DOUT/
C
	EXTERNAL	NASTY
C
C despatch on secondary command
	GOTO (100,200,300),DSECN
C
C Secondary command number out of range
	CALL MSGCMD(NASTY,MSG_ERRSECIND)
	GOTO 998
C
C AFTER INPUT
100	CONTINUE
	IF (DSTRIN) THEN
	   AFTER_INPUT_LEN = DSTRL
	   IF (DSTRL.GT.0)
     &	      CALL CPYSTR(DSTR(1:DSTRL),AFTER_INPUT(1:AFTER_INPUT_LEN))
	ELSE
	   AFTER_INPUT_LEN = 0
	ENDIF
	GOTO 999
C
C AFTER ERROR
200	CONTINUE
	IF (DSTRIN) THEN
	   AFTER_ERROR_LEN = DSTRL
	   IF (DSTRL.GT.0)
     &	      CALL CPYSTR(DSTR(1:DSTRL),AFTER_ERROR(1:AFTER_ERROR_LEN))
	ELSE
	   AFTER_ERROR_LEN = 0
	ENDIF
	GOTO 999
C
C AFTER ABORT
300	CONTINUE
	IF (DSTRIN) THEN
	   AFTER_ABORT_LEN = DSTRL
	   IF (DSTRL.GT.0)
     &	      CALL CPYSTR(DSTR(1:DSTRL),AFTER_ABORT(1:AFTER_ABORT_LEN))
	ELSE
	   AFTER_ABORT_LEN = 0
	ENDIF
	GOTO 999
C
998	CONTINUE
	CALL IGNCMD
C
999	RETURN
	END
