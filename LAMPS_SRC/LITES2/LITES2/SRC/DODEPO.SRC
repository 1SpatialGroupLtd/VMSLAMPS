	SUBROUTINE DODEPO
C
***	IDENT   14AP86
***	MODULE  DODEPO
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Written					RWR	30-Oct-198
C
C Description
C
C	LITES2 cartographic editor DEPOSIT command.
C
	IMPLICIT NONE
C
C Included Parameter Files
C
	INCLUDE	'LSL$CMNIFF:CB.STR'
***	PARAMETER/CMN:AUXDEF/
***	PARAMETER/CMN:PACK/
C
C Common Blocks
C
***	COMMON/CMN:OIIH/
***	COMMON/CMN:MODIFY/
C
C function called
C
	LOGICAL NDSTCN
C
C working storage
C
	INTEGER		SAV_FSN
C
C save settings
C
	SAV_FSN    =COIPAK(PAKFSN)
C
C will need a new segment number for this feature
C
	COIPAK(PAKSEG)=0
C
C deposit feature 
C
	IF (NDSTCN(.FALSE.)) CALL IGNCMD
C
C set things back to what they were
C
	COIPAK(PAKFSN)=SAV_FSN
	COIPAK(PAKFLG)=0
C
	RETURN
	END
