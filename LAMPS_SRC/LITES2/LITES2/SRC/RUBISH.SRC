	LOGICAL FUNCTION RUBISH(XY)
C
***	MODULE	RUBISH
***	IDENT	21FE85
C
C Copyright Laser Scan Laboratories Ltd., Cambridge, England.
C
C To check if point XY is in range
C
	IMPLICIT NONE
C
***	COMMON/CMN:MASTER/
C
C arguments
	REAL XY(2)		! the coordinates of the point
C
	RUBISH = (XY(1).LT.LIMITS(1) .OR. XY(1).GT.LIMITS(2) .OR.
     &		  XY(2).LT.LIMITS(3) .OR. XY(2).GT.LIMITS(4) )
	RETURN
	END
