	SUBROUTINE BLINE(LNE)
C
***	MODULE	BLINE
***	IDENT	01MY87
C
C Copyright Laser Scan Laboratories Ltd., Cambridge, England.
C
C To output a blank line on the VT100 screen at line LINE
C
	IMPLICIT NONE
C
C arguments
	INTEGER		LNE		! line number (1 top of screen)
	INTEGER		COL
	DATA		COL/1/
C
	CALL LIB$ERASE_LINE(LNE,COL)
C
	RETURN
	END
