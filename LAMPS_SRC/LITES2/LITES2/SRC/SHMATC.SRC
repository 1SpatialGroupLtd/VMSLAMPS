	CALL INFORM(MSG_MATFSN,ONOFF(MATFSN))
	CALL INFORM(MSG_MATLAY,ONOFF(MATLAY))
	CALL INFORM(MSG_MATMAP,ONOFF(MATMAP))
	CALL INFORM(MSG_MATPC,ONOFF(MATPC))
	CALL INFORM(MSG_MATAC,ONOFF(MATAC))
