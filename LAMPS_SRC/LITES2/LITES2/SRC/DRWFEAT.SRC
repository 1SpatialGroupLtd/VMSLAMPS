	SUBROUTINE DRWFEAT
C
***	IDENT	19AP90
***	MODULE	DRWFEAT
C
C	Copyright Laser-Scan Ltd., Cambridge, England.
C
C	Created					RWR	30-Mar-1990
C
C	LITES2 cartographic editor DRAW FEAT command.
C
	IMPLICIT NONE
C
	INCLUDE	'LSL$CMNIFF:CB.STR'
***	PARAMETER/CMN:AUXDEF/
***	PARAMETER/CMN:PACK/
***	PARAMETER/CMN:LICPAR/
***     PARAMETER/CMN:MOANMESS/		! moan constants
C
***	COMMON/CMN:DRAWIN/
***     COMMON/CMN:MASTER/
***	COMMON/CMN:RFCTRL/
***	COMMON/CMN:STRIKE/
C
C check for found object
	IF (.NOT.FO) THEN
	   CALL MOAN(MSG_NOFEATURE)
	   CALL IGNCMD
C
C draw feature
	ELSE
	   CALL SETANNO(.TRUE.)
	   FROM_DRWFEAT = .TRUE.		! for STDRAW
	   CALL DRAFTR(FOPAK)
	   FROM_DRWFEAT = .FALSE.
	   OCSXY(1) = CSXY(1)+1.0		! force refresh re-draw
	   CALL SETANNO(.FALSE.)
	ENDIF
C
	RETURN
	END
