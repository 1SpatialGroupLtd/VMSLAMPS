	SUBROUTINE WARP_REVERSE(N,PTS)
C
***	IDENT	10MY96
***	MODULE	WARP_REVERSE
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Written					CCB	15-Sep-1994
C
C	Reverse a set of transformation points
C
        IMPLICIT NONE
C
C Arguments
	INTEGER	N
	REAL*4	PTS(4,*)
C
C workspace
	INTEGER	I
	REAL	R
C
	DO 10 I=1,N
	   R = PTS(1,I)
	   PTS(1,I) = PTS(3,I)
	   PTS(3,I) = R
	   R = PTS(2,I)
	   PTS(2,I) = PTS(4,I)
	   PTS(4,I) = R
10	CONTINUE

	RETURN
	END
