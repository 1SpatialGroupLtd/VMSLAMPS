	SUBROUTINE STRINS(SOURCE,DEST,POS)
C
***	MODULE	STRINS
***	IDENT	02OC84
C
C Copyright Laser Scan Laboratories Ltd., Cambridge, England.
C
C To insert string SOURCE into string DEST at position POS
C
	IMPLICIT NONE
C
C arguments
	CHARACTER*(*)	SOURCE,DEST
	INTEGER		POS
C
	DEST(POS:POS+LEN(SOURCE)-1) = SOURCE
	RETURN
C
	END
