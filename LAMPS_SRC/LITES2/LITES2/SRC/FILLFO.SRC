	SUBROUTINE FILLFO(NOPTS,NOACS,FSN,FC,MAP,LAYER,GT,ROTAT,THICK)
C
***	IDENT	01DE88
***	MODULE	FILLFO
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Created					RWR	28-Jun-1985
C
C Description
C
C	Subroutine to extract important things from found object,
C	if there is one.
C
	IMPLICIT NONE
C
C
C Included Parameter Files
C
	INCLUDE	'LSL$CMNIFF:CB.STR'
***	PARAMETER/CMN:AUXDEF/
***	PARAMETER/CMN:CTXS/
***	PARAMETER/CMN:PACK/
C
C Common Blocks
C
***	COMMON/CMN:CTXS/
***	COMMON/CMN:LAYERS/
***	COMMON/LSL$CMNFRT:FRTCOM.CMN/
***	COMMON/CMN:STRIKE/
C
C Arguments
C
	INTEGER*4	NOPTS
	INTEGER*4	NOACS
	INTEGER*4	FSN
	INTEGER*4	FC(4)			! NOTE: INTEGER*4
	INTEGER*4	MAP
	INTEGER*4	LAYER
	INTEGER*4	GT
	REAL		ROTAT			! rotation of text or
						! symbol
	INTEGER*4	THICK
C
C working storage
C
	INTEGER*4	I,MASK
C
	MASK='FFFF'X
	IF (FO) THEN
	   NOPTS=FOLST
	   FSN=FOPAK(PAKFSN)
	   DO 10,I=1,4
	      FC(I)=FOFS(I).AND.MASK
   10	   CONTINUE
	   MAP=FOPAK(PAKMAP)+1
	   LAYER=OVNOS(FOPAK(PAKOVL))
	   GT =FOPAK(PAKGT)
	   IF (GT.GE.ORITYP.AND.GT.LT.TEXTYP) THEN
	      ROTAT=FOAUX(ANGI)
	   ELSE
	      ROTAT=0.0
	   ENDIF
	   IF (GT.EQ.TEXTYP) THEN
C
C get first text into FTRCTXD
	      FTRCTXD(CTXD_CUR) = FTRCTXD(CTXD_FSTCTX)
	      CALL THISCTX(FTRCTXD)
	      CALL PUTLNG(CTXBUFL(CTX_L_TH),THICK)
	      CALL PUTLNG(CTXBUFL(CTX_L_RO),ROTAT)
C
C number of points is number of subtexts
	      NOPTS = FTRCTXD(CTXD_NCTX)
	   ELSE
	      THICK=0
	   ENDIF
	ELSE
	   NOPTS=0
	   NOACS=0
	   FSN = 0
	   DO 20, I=1,4
	      FC(I) = 0
   20	   CONTINUE
	   MAP = 0
	   LAYER = 0
	   GT = 0
	   ROTAT=0.0
	   THICK=0
	ENDIF
C
C Return
C
	RETURN
	END
