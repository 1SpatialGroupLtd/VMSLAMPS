	SUBROUTINE D14COF
C
***	MODULE	D14COF
***	IDENT	15AP86
C
	IMPLICIT NONE
C
C Created by Alan Wylie 5-Feb-1980
C From the Ordnance Survey CORAL66 squaring program
C
C Compute line coefficients
C
***	COMMON/CMN:D14IFC/
***	COMMON/CMN:D14CMN/
C
	DX=X2-X1
	DY=Y2-Y1
	IF (DX.EQ.0.0.AND.DY.EQ.0.0) THEN
	   A1=1
	   A2=0
	ELSE
	   IF (ABS(DX).GE.ABS(DY)) THEN
	      A1=-DY/DX
	      A2=1.0
	   ELSE
	      A1=1.0
	      A2=-DX/DY
	   ENDIF
	ENDIF
C
	RETURN
	END
