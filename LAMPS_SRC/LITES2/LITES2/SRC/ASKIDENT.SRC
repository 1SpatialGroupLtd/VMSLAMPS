	SUBROUTINE ASKIDENT
C
***	IDENT	19JN90
***	MODULE	ASKIDENT
C
C	Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C	Created					RWR	31-May-1990
C
C	LITES2 cartographic editor ASK IDENT command.
C
	IMPLICIT NONE
C
***     PARAMETER/CMN:MOANMESS/		! moan constants
C
	INCLUDE	'($CHPDEF)'
	INCLUDE	'($SSDEF)'
C
***     COMMON/CMN:DOUT/		! command decoding output
***     COMMON/CMN:MASTER/
C
C Functions
	INTEGER SYS$PARSE_ACL,SYS$CHKPRO
C
C Local Variables
	INTEGER	OK
C
	STRUCTURE /ITEM/
	   UNION
	   MAP
	      INTEGER*2 BUFLEN
	      INTEGER*2 CODE
	   END MAP
	   MAP
	      INTEGER*4 END
	   END MAP
	   END UNION
	   INTEGER*4 BUFADR
	   INTEGER*4 RETLENADR
	END STRUCTURE
C
	RECORD/ITEM/ITMLST(2)
	CHARACTER*(12)		ACLENT
C
	IF (DSTRL.GT.0) THEN
C 
	   OK = SYS$PARSE_ACL('(IDENTIFIER='//DSTR(1:DSTRL)//')',
     &					ACLENT,,,)
	   IF (OK.EQ.SS$_NOSUCHID) THEN
	      CALL MOAN(MSG_NOSUCHID,DSTR(1:DSTRL))
	      GOTO 9990
	   ELSEIF (.NOT.OK) THEN
	      CALL MOAN(MSG_ERRINASKIDENT,DSTR(1:DSTRL))
	      CALL LITES2_SIGNAL(OK)
	      GOTO 9990
	   ENDIF
	ELSE
	   CALL MOAN(MSG_NOSUCHID,' ')
	   GOTO 9990
	ENDIF
C
	ITMLST(1).BUFLEN = ICHAR(ACLENT(1:1))
	ITMLST(1).CODE = CHP$_ACL
	ITMLST(1).BUFADR = %LOC(ACLENT)
	ITMLST(2).END = CHP$_END
C
	OK = SYS$CHKPRO(ITMLST)
	IF (OK) THEN
	   ASK_INT(1) = -1			! success
	   ASK_INT_NUM = 1
	ELSEIF (OK.EQ.SS$_NOPRIV) THEN
	   ASK_INT(1) = 0
	   ASK_INT_NUM = 1
	ELSE
	   CALL MOAN(MSG_ERRINASKIDENT,DSTR(1:DSTRL))
	   CALL LITES2_SIGNAL(OK)
	   GOTO 9990
	ENDIF
C
	GOTO 9999
C
9990	CALL IGNCMD
9999	RETURN
	END
