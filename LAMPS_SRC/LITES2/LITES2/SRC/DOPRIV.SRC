        SUBROUTINE DOPRIV
C
***     IDENT   20JL88
***     MODULE  DOPRIV
C
C       Copyright Laser-Scan Laboratories Ltd., Cambridge, England.
C
C       Written                                 RWR	24-JUN-1988
C
C	LITES2 cartographic editor PRIVILEGE command.
C
        IMPLICIT NONE
C
***	PARAMETER/CMN:MOANMESS/
C
***	COMMON/CMN:DOUT/
***	COMMON/CMN:GLOBAL/
***	COMMON/CMN:MASTER/
C
C functions
	LOGICAL		PRIV_COMMAND
	LOGICAL		PRIV_ATTR
C
C w/s
	EXTERNAL 	MOAN,NASTY
C
C Dispatch according to secondary command
C
	GOTO (100,200,300), DSECN
C
C Secondary command number out of range
C
	CALL MSGCMD(NASTY,MSG_ERRSECIND)
	GOTO 998
C
C PRIVILEGE COMMAND
100	CONTINUE
	IF (PRIV_COMMAND()) GOTO 998
	GOTO 999
C
C PRIVILEGE ATTRIBUTE
200	CONTINUE
	IF (PRIV_ATTR(PRIV_ATTR_LIST,.FALSE.)) GOTO 998
	GOTO 999
C
C PRIVILEGE POINT
300	CONTINUE
	IF (PRIV_ATTR(PRIV_PT_LIST,.TRUE.)) GOTO 998
	GOTO 999
C
998	CONTINUE
	CALL IGNCMD
C
999	CONTINUE
        RETURN
        END
