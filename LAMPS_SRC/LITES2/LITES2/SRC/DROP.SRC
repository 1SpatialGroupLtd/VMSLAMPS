	SUBROUTINE DROP
C
***	MODULE	DROP
***	IDENT	26FE85
C
C Copyright Laser Scan Laboratories Ltd., Cambridge, England.
C Author Clarke Brunt
C
C drops the current found item
C
	IMPLICIT NONE
C
	INCLUDE	'LSL$CMNIFF:CB.STR'
***	PARAMETER/CMN:AUXDEF/
***	PARAMETER/CMN:PACK/
C
***	COMMON/CMN:MASTER/
***	COMMON/CMN:STRIKE/
C
	FO = .FALSE.			! no found object
	HITITC = 0			! no hit items
	IF (ROLING.AND.FNDMDE.EQ.1) CALL FRECUR	! unconstrain
	RETURN
	END
