! OSINIT.LCM - OS LITES2 terminal-independent initialisation file
! see also individual terminal-specific files eg TTA6.LCM
!
enable height		! text heights
enable learner		! extra messages
disable now		! status line so no "Now in ..." messages
fiducial layer 0,11	! grid in layer 11
verify text
setup OS
OSSETUP 5 5 4 2 19.05	! OS style map setup
@OSTXTMEN
@OSFCSMEN
@OSCMDMEN
frt OS
sri OS
tri OS
