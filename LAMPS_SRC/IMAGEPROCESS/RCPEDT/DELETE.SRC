	SUBROUTINE DELETE(LINE)
C
***	MODULE	DELETE
***	IDENT	23NO88
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England
C Created						ACM	Feb-1988
C
	IMPLICIT	NONE
C
C Parameters
***	PARAMETER/LSL$CMNRCPEDT:RCPEDTMSG/
C
C Commons
***	COMMON/LSL$CMNRCPEDT:RCPEDT/
C
C Data in
	INTEGER*4	LINE		! line number to delete
C
C Workspace
	LOGICAL*4	COMMENT_LINE	! line LINE is comment line?
	INTEGER*4	NUM		! CP or comment index
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C	Deletes CP or comment on line LINE by overwriting arrays.
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
	IF (LINE.LT.1.OR.LINE.GT.N_LINES) THEN
	  CALL LSL_PUTMSG(RCPEDT__NOSUCHLINE,N_LINES)
	  GOTO 999
	END IF
C
C Get CP or comment index
C
	NUM = RCP_CMT_INDEX(LINE)
C
C Check whether line is a CP or a comment line
C
	COMMENT_LINE = NUM.LT.0
C
C Call appropriate DELETE routine
C
	IF (COMMENT_LINE) THEN
	  CALL DELETE_COMMENT(LINE)
	ELSE
	  CALL DELETE_RCP(LINE)
	END IF
C
999	RETURN
	END
