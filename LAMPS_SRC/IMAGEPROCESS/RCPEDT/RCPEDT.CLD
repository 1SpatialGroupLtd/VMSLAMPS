	MODULE	RCPEDT_CLD

***	MODULE	RCPEDT_CLD
***	IDENT	18NO88

	DEFINE	VERB  RCPEDT

	ROUTINE	RCPEDT

	QUALIFIER	NEW,NONNEGATABLE,
 			VALUE(REQUIRED,TYPE=$FILE)

	QUALIFIER	OLD,NONNEGATABLE,
 			VALUE(REQUIRED,TYPE=$FILE)

	QUALIFIER	UPDATE,NONNEGATABLE,
 			VALUE(TYPE=$FILE)

	DISALLOW	NEW AND OLD
	DISALLOW	NEW AND UPDATE
	DISALLOW	UPDATE AND NOT OLD
