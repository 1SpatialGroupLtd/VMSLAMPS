***	MODULE	READCCT_CLD
***	IDENT	16MA87


	DEFINE VERB	READCCT

	ROUTINE		READCCT		

	QUALIFIER	ANCILLARY,NEGATABLE

	QUALIFIER	ANNOTATIONS,NEGATABLE

	QUALIFIER	BANDS VALUE(LIST,TYPE=$QUOTED_STRING,REQUIRED)

	QUALIFIER	CALIBRATION

	QUALIFIER	NOCALIBRATION

	QUALIFIER	DESCRIPTOR,NEGATABLE

	QUALIFIER	DEVICE VALUE(TYPE=$QUOTED_STRING,REQUIRED)

	QUALIFIER	DIAGNOSTICS,NEGATABLE

	QUALIFIER	DTI VALUE(TYPE=$QUOTED_STRING,
                                  DEFAULT=LSL$DTI:SPOT.DTI)

	QUALIFIER	END_LINE VALUE(TYPE=$QUOTED_STRING,REQUIRED)

	QUALIFIER	END_SAMPLE VALUE(TYPE=$QUOTED_STRING,REQUIRED)

