        LOGICAL FUNCTION READ_TRAILER()
C
***	MODULE	READ_TRAILER
***	IDENT	06SE88
C
C 	Copyright Laser-Scan Laboratories Ltd.,Cambridge,England
C 	Author: D R Catlow
C
C	Routine for READCCT to decode a trailer record
C	A number of different trailer records may exist.
C	The record number determines how they are decoded.
C	2 = Trailer Quality Record
C	3 = Geometric Transformation Record
C	4 = Grid Parameter Record
C	5+ = Grid Rectification Record(s)
C
C
	IMPLICIT NONE
C
	INCLUDE 'HERE:READCCT.CMN'
	INCLUDE 'HERE:TAPE.CMN'		!Values relating to the tape records
	INCLUDE 'HERE:TAPE_RECORDS.CMN'
C
	INTEGER*4	TRAILER_NUMBER	!trailer record number
	INTEGER*4	IERR
	REAL*8		REAL_VALUE	!double precision real
	REAL*4		REAL_VAL	!single precision real
	INTEGER*4	I		!counter
	INTEGER*4	POINTER 	!decode pointer into MTBUF
C
C	Functions:
	INTEGER*4	FORM_SPOT_REAL4	!to form single precision real	
	INTEGER*4	FORM_SPOT_REAL8	!to form double precision rela
	INTEGER*4	FLWSEL		!to select file
C----------------------------------------------------------------------------
C
	COUNT_TRAILER=COUNT_TRAILER+1	!increment counter for this
C					!record type
C
	READ_TRAILER=.TRUE.		!assume successful
C
	IF(.NOT.READ_TRAILER)GOTO 999	!don't decode this record
C
	IF(SKIP_SCENE)GOTO 999
C---------------------------------------------------------------------------
C
C	Trailer Record Number
	CALL FORM_LONGWORD(TRAILER_NUMBER,1)
C
	GOTO (999,200,300,500)TRAILER_NUMBER
	IF(TRAILER_NUMBER.GT.5)GOTO 600
	GOTO 999
C
C---------------------------------------------------------------------------
C	Trailer Quality Record
200	IF((HAD_PRINT.OR.HAD_LIST).AND.SPOT)THEN
C
	  IERR=FLWSEL(2)
	  IF(.NOT.IERR)THEN
	    CALL LSL_PUTMSG(IERR)
	    READ_TRAILER=.FALSE.
	    GOTO 999
	  ENDIF
C
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
	  CALL FLWRTF
     &  ('            *** SPOT TRAILER QUALITY RECORD  ***')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  CALL FLWRTF
     &  ('Number of write parity errors recovered    : %A4',MTBUF(21))
	  CALL FLWRTF
     &  ('Image Quality                              :')
	  CALL FLWRTF
     &  ('%A50',MTBUF(35))
	  CALL FLWRTF
     &  ('%A50',MTBUF(85))
	  CALL FLWRTF
     &  ('%A50',MTBUF(135))
	  CALL FLWRTF
     &  ('%A50',MTBUF(185))
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
	  CALL FLWRTF
     &  ('Number of Trailer Records:')
	  CALL FLWRTF
     &  ('Geometric Transformation Records           : %A4',MTBUF(235))
	  CALL FLWRTF
     &  ('Grid Parameter Records                     : %A4',MTBUF(275))
	  CALL FLWRTF
     &  ('Rectification Grid Records                 : %A4',MTBUF(295))
	ENDIF
C
	GOTO 999
C
C-------------------------------------------------------------------------
C	Geometric Transformations Record
300	IF((HAD_PRINT.OR.HAD_LIST).AND.SPOT)THEN
C
	  IERR=FLWSEL(2)
	  IF(.NOT.IERR)THEN
	    CALL LSL_PUTMSG(IERR)
	    READ_TRAILER=.FALSE.
	    GOTO 999
	  ENDIF
C
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
	  CALL FLWRTF
     & ('  *** SPOT TRAILER GEOMETRIC TRANSFORMATION RECORD ***')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  CALL FLWRTF
     & ('DIRECT LOCATION MODELS')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  POINTER=21
	  DO 310 I=1,10
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the X Model         :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
310       CONTINUE
C
	  POINTER=101
	  DO 320 I=1,10
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the Y Model         :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
320       CONTINUE
C
	  POINTER=181
	  DO 330 I=1,10
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the Z Model         :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
330       CONTINUE
C
	  CALL FLWRTF
     & ('REVERSE LOCATION MODELS')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  POINTER=261
	  DO 340 I=1,4
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the line (l) Model  :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
340       CONTINUE
C
	  POINTER=293
	  DO 350 I=1,4
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the pixel (p) Model :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
350	  CONTINUE
C
	  CALL FLWRTF
     & ('HYBRID MODELS')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  POINTER=325
	  DO 360 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 1st Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
360	  CONTINUE
C
	  POINTER=349
	  DO 370 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 2nd Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
370	  CONTINUE
C
	  POINTER=373
	  DO 380 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 3rd Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
380	  CONTINUE
C
	  POINTER=397
	  DO 390 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 4th Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
390	  CONTINUE
C
	  POINTER=421
	  DO 400 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 5th Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
400	  CONTINUE
C
	  POINTER=445
	  DO 410 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 6th Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
410	  CONTINUE
C
	  POINTER=469
	  DO 420 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 7th Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
420	  CONTINUE
C
	  POINTER=493
	  DO 430 I=1,3
	    IERR = FORM_SPOT_REAL8(REAL_VALUE,POINTER)
	    IF(.NOT.IERR)REAL_VALUE=0.0
	    POINTER=POINTER+8
	    IF(I.EQ.1)THEN
	      CALL EXPAND
     &  ('Coefficients for the 8th Model       :')
	    ELSE
	      CALL EXPAND
     &  ('                                     :')
	    ENDIF
	    CALL APPEND('%^D%E16',REAL_VALUE)
	    CALL FLWLIN
430	  CONTINUE
C
	  GOTO 999
	ENDIF
C
C----------------------------------------------------------------------
C	Grid Parameter Record
500	IF((HAD_PRINT.OR.HAD_LIST).AND.SPOT)THEN
C
	  IERR=FLWSEL(2)
	  IF(.NOT.IERR)THEN
	    CALL LSL_PUTMSG(IERR)
	    READ_TRAILER=.FALSE.
	    GOTO 999
	  ENDIF
C
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
	  CALL FLWRTF
     &  ('         *** SPOT GRID PARAMETER TRAILER RECORD  ***')
	  CALL FLWRTF
     & ('-------------------------------------------------------------')
C
	  CALL FLWRTF
     &  ('Number of Grid Lines                       : %A3',MTBUF(27))
	  CALL FLWRTF
     &  ('Number of Grid Columns                     : %A3',MTBUF(30))
	  CALL FLWRTF
     &  ('Grid step along Columns                    : %A3',MTBUF(45))
	  CALL FLWRTF
     &  ('Grid step along Lines                      : %A3',MTBUF(48))
C
	  GOTO 999
	ENDIF
C
C----------------------------------------------------------------------
C	Grid Rectification Records
600	GRID_RECTIFICATION = GRID_RECTIFICATION + 1
C
	IF((HAD_PRINT.OR.HAD_LIST).AND.SPOT)THEN
C
	  IERR=FLWSEL(2)
	  IF(.NOT.IERR)THEN
	    CALL LSL_PUTMSG(IERR)
	    READ_TRAILER=.FALSE.
	    GOTO 999
	  ENDIF
C
	  IF(GRID_RECTIFICATION.EQ.1)THEN
	    CALL FLWRTF
     & ('-------------------------------------------------------------')
	    CALL FLWRTF
     &  ('      *** SPOT GRID RECTIFICATION TRAILER RECORDS  ***')
	    CALL FLWRTF
     & ('-------------------------------------------------------------')
	    CALL FLWRTF
     &  ('Rectification Grid Coordinates             :')
	    CALL FLWRTF
     & ('-------------------------------------------------------------')
	  ENDIF
C
	  POINTER=21
	  DO 700 I=1,66
C value 1
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL EXPAND('%E',REAL_VAL)
C
C value 2
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C	     
C value 3
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C value 4
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C value 5
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C value 6
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C value 7
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C value 8
	     IERR = FORM_SPOT_REAL4(REAL_VAL,POINTER)
	     IF(.NOT.IERR)REAL_VAL=0.0
	     POINTER=POINTER+4
	     CALL APPEND('%E',REAL_VAL)
C
C output these 8 values
	     CALL FLWLIN
C
700	   CONTINUE
C
	ENDIF
C
C
C----------------------------------------------------------------------
999	RETURN
C
	END
