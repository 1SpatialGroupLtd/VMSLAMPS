	INTEGER*4 FUNCTION TIDYUP
C
***	MODULE	TIDYUP
***	IDENT	09DE86
C
C Copyright Laser-Scan Laboratories Ltd., Cambridge, England
C
C Author M.W.S. Reid
C
C 	Created					MWSR    18-Nov-1986
C
C closes input and output DTI files for DTIFILTER
C
C
	IMPLICIT NONE
C
C Parameters
***	COMMON/LSL$CMNDTI:DTILIBMSG.PAR/
***	COMMON/LSL$CMNDTIFILTER:DTIFILTERMSG.PAR/
C
C Common Blocks
***	COMMON/LSL$CMNDTI:DTIHDR/
***	COMMON/LSL$CMNDTIFILTER:DFILCMN/
***	COMMON/LSL$CMNDTIFILTER:LKPCMN/
C
C Functions
	INTEGER*4	DTI_MINMAX_DATA
	INTEGER*4	DTI_CLOSE
	INTEGER*4	FLWCLO
C
C Local Varaiables
	INTEGER*4	LUN
	INTEGER*4	ERR
	INTEGER*4	RET
	INTEGER*4	UNIT
C
C
C first calculate new min and max values
C
	LUN = DF_OUTLUN
	IF (DF_HADINP) THEN
	   TIDYUP = DTI_MINMAX_DATA(LUN,.TRUE.,ERR)
	   IF (.NOT. TIDYUP) THEN 
	      CALL LSL_PUTMSG(ERR)
	      GOTO 999
	   ENDIF
	ENDIF
C
C close input file
C
	IF (DF_HADINP) THEN
	   TIDYUP = DTI_CLOSE(DF_INLUN,.FALSE.,.FALSE.,ERR)
	   IF (.NOT. TIDYUP) THEN
	      IF (TIDYUP .EQ. DTI__SYSERR) THEN
	        CALL LSL_PUTMSG(ERR)
	      ELSE
		CALL LSL_PUTMSG(TIDYUP)
	      ENDIF
	   ENDIF
	ENDIF
C
C close output file
	IF (DF_HADOUP) THEN
	   TIDYUP = DTI_CLOSE(DF_OUTLUN,.TRUE.,.FALSE.,ERR)
	   IF (.NOT. TIDYUP) THEN
	      IF (TIDYUP .EQ. DTI__SYSERR) THEN
	        CALL LSL_PUTMSG(ERR)
	      ELSE
		CALL LSL_PUTMSG(TIDYUP)
	      ENDIF
	   ENDIF
	ENDIF
C
C close look up file
	IF (LKP_HADTABLE) THEN
	    UNIT = 3
	    RET= FLWCLO(UNIT,ERR)
	    IF (.NOT. RET) THEN
	       CALL LSL_PUTMSG(DTIFIL__ERRCLOLKP)
	       CALL LSL_ADDMSG(ERR)
	       GOTO 999
	    ENDIF
	ENDIF
C
999	RETURN
	END
