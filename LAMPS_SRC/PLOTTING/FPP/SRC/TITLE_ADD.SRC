	SUBROUTINE TITLE_ADD(C)
C
***	MODULE	TITLE_ADD
***	IDENT	29SE87
C
C Copyright Laser Scan Laboratories Limited, Cambridge, England
C
C Add string to title
C
	IMPLICIT NONE
C
***	COMMON/CMN:TITLE/
C
C argument
	CHARACTER*(*)	C
C
C W/s.
 	INTEGER	  I
C
	I = MIN(TITLE_MAXTEXTLEN-TITLE_TEXTLEN,LEN(C))
	IF (I.GT.0) THEN
	   I = I + TITLE_TEXTLEN
	   TITLE_TEXT(TITLE_TEXTLEN+1:I) = C
	   TITLE_TEXTLEN = I
	ENDIF
C
	RETURN
	END
