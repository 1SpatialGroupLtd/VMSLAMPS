C
C Device dependent block data for Benson 3000 metafile version of FPP
C						(FPPBENMET)
C If setting anything here already set in BLKDAT, then be careful
C to link with this last.
C
	CHARACTER*(*)	FPPVER
	PARAMETER	(FPPVER='BENMET')
	CHARACTER*(*)	HCBSVER
	PARAMETER	(HCBSVER='$$$$')
C
C set workstation type to roll paper Calcomp for LDCPLIB
C Number is 100 + 1 bit for sheet + 2 bit for Benson
	DATA	WTYPE/100/
C
C no hardware symbols or hardware line patterns
	DATA	HWLNS,HWSYS,HWESC/0,0,1/
C
