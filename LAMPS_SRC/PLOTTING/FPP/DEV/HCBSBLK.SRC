C
C Device dependent block data for Calcomp HCBS version of FPP (FPPHCBS)
C If setting anything here already set in BLKDAT, then be careful
C to link with this last.
C
	CHARACTER*(*)	FPPVER
	PARAMETER	(FPPVER='HCBS')
	CHARACTER*(*)	HCBSVER
	PARAMETER	(HCBSVER='$$$$')
C
C set workstation type to sheet paper Calcomp for LDCPLIB
C Number is 100 + 1 bit for sheet + 2 bit for Benson
	DATA	WTYPE/101/
C
C no hardware symbols or hardware line patterns
	DATA	HWLNS,HWSYS/0,0/
C
C allow hardware text
	DATA	HWCIR,HWPAT,HWCUR,HWTXT/.FALSE.,.FALSE.,.FALSE.,.TRUE./
C
